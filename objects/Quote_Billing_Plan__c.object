        <externalId>false</externalId>
        <formula>AND( 
   OR(TEXT(Quote__r.Order__r.status__c) = &apos;Approved&apos;, TEXT(Quote__r.Order__r.status__c) = &apos;Completed&apos;),
      Invoice_Date__c &gt;= DATE( YEAR(TODAY()), 
                               CASE(MONTH(TODAY()), 
                                     1, 1, 
                                     2, 1,
                                     3, 1,
                                     4, 4, 
                                     5, 4,
                                     6, 4,
                                     7, 7, 
                                     8, 7,
                                     9, 7,
                                    10, 10, 
                                    11, 10,
                                    12, 10,
                                    1), 
                               1)
   )</formula>
