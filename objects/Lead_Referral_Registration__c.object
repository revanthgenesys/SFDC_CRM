        <linkType>javascript</linkType>
        <masterLabel>Convert to Opportunity</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&apos;/soap/ajax/29.0/connection.js&apos;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/22.0/apex.js&quot;)}

if ( &quot;{!Lead_Referral_Registration__c.Status__c}&quot; != &quot;Approved&quot; ) {
alert (&quot;This Registration has not yet been Approved.&quot;);
}
else if ( &quot;{!Lead_Referral_Registration__c.End_User_Account__c}&quot; == &quot;&quot; ) {
alert (&quot;Please provide End User Account.&quot;);
}
/*
else if ( &quot;{!Lead_Referral_Registration__c.Primary_Contact__c}&quot; == &quot;&quot; ) {
  alert (&quot;Please provide Primary Contact.&quot;);
}*/
else if ( &quot;{!Lead_Referral_Registration__c.Opportunity__c}&quot; != &quot;&quot; ) {
alert (&quot;There is already an Opportunity created for this Registration.&quot;);
}
else if ( confirm(&quot;This will create an Opportunity from the selected Registration. Click Yes to confirm.&quot;) ) {
var result = sforce.apex.execute(&quot;OpportunityNewFromRegistration&quot;,&quot;newOppFromOppRegistr&quot;,
{
objId: &quot;{!Lead_Referral_Registration__c.Id}&quot;,
objType: &quot;LeadOppRegistration&quot;
});

alert(result);
}

location.href=&quot;/{!Lead_Referral_Registration__c.Id}&quot;;</url>
