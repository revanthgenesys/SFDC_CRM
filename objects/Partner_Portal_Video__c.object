    <allowInChatterGroups>false</allowInChatterGroups>
    <compactLayoutAssignment>SYSTEM</compactLayoutAssignment>
    <deploymentStatus>Deployed</deploymentStatus>
    <description>Used to store the links to videos to display in the partner portal.</description>
    <enableActivities>false</enableActivities>
    <enableBulkApi>true</enableBulkApi>
    <enableFeeds>false</enableFeeds>
    <enableHistory>true</enableHistory>
    <enableLicensing>false</enableLicensing>
    <enableReports>false</enableReports>
    <enableSearch>true</enableSearch>
    <enableSharing>true</enableSharing>
    <enableStreamingApi>true</enableStreamingApi>
    <externalSharingModel>ReadWrite</externalSharingModel>
    <fields>
        <fullName>Expiration_Date__c</fullName>
        <description>The date and time the video will be removed from displays.  If omitted, the video will be visible until it is deleted.</description>
        <externalId>false</externalId>
        <label>Video Expiration Date</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>DateTime</type>
    </fields>
    <fields>
        <fullName>Home_Page_Flag__c</fullName>
        <defaultValue>false</defaultValue>
        <description>Checked if the video is eligible for display on the Partner Portal Home Page.</description>
        <externalId>false</externalId>
        <inlineHelpText>If this box is checked, the video will be eligible for display on the Partner Portal Home Page.  The video chosen for display will be selected at random from videos currently visible and flagged with this checkbox.</inlineHelpText>
        <label>Video Used on Home Page</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Publication_Date__c</fullName>
        <description>The date and time the video will become visible on displays.  If omitted, the video will be published immediately.</description>
        <externalId>false</externalId>
        <label>Video Publication Date</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>DateTime</type>
    </fields>
    <fields>
        <fullName>Title__c</fullName>
        <description>Title of the video for display purposes.</description>
        <externalId>false</externalId>
        <label>Video Title</label>
        <length>255</length>
        <required>true</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Video_Link_Url__c</fullName>
        <externalId>false</externalId>
        <formula>HYPERLINK(&apos;/partner/Partner_Portal_Video_Page?id=&apos;+ Id ,  Title__c )</formula>
        <label>Video Link Url</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Video_Link__c</fullName>
        <description>URL or *other* identifier for videos.  It must contain enough information for the video handler to determine the type of video (i.e., YouTube vs. Video Cloud)</description>
        <externalId>false</externalId>
        <label>Video Link or ID</label>
        <length>2048</length>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>LongTextArea</type>
        <visibleLines>2</visibleLines>
    </fields>
    <fields>
        <fullName>Video_Type__c</fullName>
        <externalId>false</externalId>
        <label>Video Type</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Picklist</type>
        <valueSet>
            <valueSetDefinition>
                <sorted>false</sorted>
                <value>
                    <fullName>Youtube</fullName>
                    <default>true</default>
                    <label>Youtube</label>
                </value>
                <value>
                    <fullName>Brightcove</fullName>
                    <default>false</default>
                    <label>Brightcove</label>
                </value>
            </valueSetDefinition>
        </valueSet>
    </fields>
    <label>Partner Portal Video</label>
    <listViews>
        <fullName>Partner_Portal_Video_List</fullName>
        <columns>Video_Link_Url__c</columns>
        <columns>Publication_Date__c</columns>
        <columns>Title__c</columns>
        <columns>Expiration_Date__c</columns>
        <filterScope>Everything</filterScope>
        <filters>
            <field>Expiration_Date__c</field>
            <operation>greaterThan</operation>
            <value>TODAY</value>
        </filters>
        <filters>
            <field>Publication_Date__c</field>
            <operation>lessThan</operation>
            <value>TODAY</value>
        </filters>
        <label>Partner Portal Video List</label>
        <language>en_US</language>
    </listViews>
    <nameField>
        <displayFormat>PPV-{000000}</displayFormat>
        <label>Video Number</label>
        <trackHistory>false</trackHistory>
        <type>AutoNumber</type>
    </nameField>
    <pluralLabel>Partner Portal Videos</pluralLabel>
    <searchLayouts/>
    <sharingModel>ReadWrite</sharingModel>
    <visibility>Public</visibility>
</CustomObject>
