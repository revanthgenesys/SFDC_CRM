    <validationRules>
        <fullName>Check_Email_Domain</fullName>
        <active>true</active>
        <errorConditionFormula>AND($User.BypassValidationRules__c = FALSE,OR(CONTAINS(Corporate_Email__c  , &apos;gmail&apos;),CONTAINS(Corporate_Email__c  , &apos;hotmail&apos;),CONTAINS(Corporate_Email__c  , &apos;yahoo&apos;)))</errorConditionFormula>
        <errorDisplayField>Corporate_Email__c</errorDisplayField>
        <errorMessage>No Gmail, Hotmail, Yahoo account is accepted.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Closed_ExpirationDate</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT $User.BypassValidationRules__c, 
ISPICKVAL(Status__c,&apos;Closed&apos;),
OR(ISPICKVAL(Sub_Status__c,&apos;Temp Exception Activation File&apos;),ISPICKVAL(Sub_Status__c,&apos;PPU Activation File&apos;),ISPICKVAL(Sub_Status__c,&apos;Eval Activation File&apos;),ISPICKVAL(Sub_Status__c,&apos;Migration Activation File&apos;)),
 RecordType.DeveloperName != &apos;Email_to_LicenseRequest&apos;, 
ISBLANK(File_Expiration_Date__c))</errorConditionFormula>
        <errorDisplayField>File_Expiration_Date__c</errorDisplayField>
        <errorMessage>File Expiration Date required for this SubStatus</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Lincense_File_Type_Required</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT $User.BypassValidationRules__c,
ISPICKVAL(Request_Type__c,&apos;Activation File Request&apos;),
ISPICKVAL( License_File_Type__c,&apos;&apos;),
 RecordType.DeveloperName != &apos;Email_to_LicenseRequest&apos;
)</errorConditionFormula>
        <errorDisplayField>License_File_Type__c</errorDisplayField>
        <errorMessage>Please select Activation File Type.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Referred_to_Other_Dept</fullName>
        <active>true</active>
        <description>Closed, Referred to Other Department  The Referred Department field must be filled out</description>
        <errorConditionFormula>AND($User.BypassValidationRules__c = FALSE,
ISPICKVAL(Status__c, &apos;Closed&apos;),
ISPICKVAL(Sub_Status__c,&apos;Referred to Other Department&apos;),
ISPICKVAL(Referred_Department__c,&apos;&apos;),
 RecordType.DeveloperName != &apos;Email_to_LicenseRequest&apos;
)</errorConditionFormula>
        <errorDisplayField>Referred_Department__c</errorDisplayField>
        <errorMessage>Please specify Referred Department</errorMessage>
    </validationRules>
