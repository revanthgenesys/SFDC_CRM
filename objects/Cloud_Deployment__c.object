    <fields>
        <fullName>Billing_Platform_Id_Hash__c</fullName>
        <description>Single Hash to identify the Billing Rating Activity. Should eventually condense all of the lower hashes into their specific definitions to the referenced  _Hash_ fields can be deleted.</description>
        <externalId>false</externalId>
        <formula>if (TEXT( Cloud_Platform__c ) == &apos;PureConnect Subscription&apos;, &apos;PCS#&apos; + Billing_Platform_Id__c, &apos;&apos;) + 
if (TEXT( Cloud_Platform__c ) == &apos;PureEngage Subscription&apos;, &apos;PES#&apos; + Billing_Platform_Id__c, &apos;&apos;) +

/* Future work to condense all previous hashes into one location */
/*
if (TEXT( Cloud_Platform__c ) == &apos;PureEngage Cloud&apos;, BE_Tenant_Id_Hash__c, &apos;&apos;) +
if (TEXT( Cloud_Platform__c ) == &apos;PureCloud&apos;, PureCloud_OrgID_Hash__c, &apos;&apos;) +
if (TEXT( Cloud_Platform__c ) == &apos;Premier Edition&apos;, PE_Subscriber_Id_Hash__c, &apos;&apos;) +
if (TEXT( Cloud_Platform__c ) == &apos;Outbound Engagement&apos;, Portico_Platform_Id_Hash__c, &apos;&apos;) +
if (TEXT( Cloud_Platform__c ) == &apos;PureConnect Cloud&apos;, PureConnect_ID_Hash__c, &apos;&apos;)
*/


/* This work will prioritize the use of the new field Billing_Platform_Id__c but for now default to the old User fields */
if (TEXT(Cloud_Platform__c)==&apos;PureEngage Cloud&apos;, IF(!ISBLANK(Billing_Platform_Id__c), &apos;BEEEC#&apos;+Billing_Platform_Id__c, IF(!ISBLANK(Tenant_ID__c), &apos;BEEEC#&apos;+Tenant_ID__c, &apos;&apos;)), &apos;&apos;) +
if (TEXT(Cloud_Platform__c)==&apos;PureCloud&apos;, IF(!ISBLANK(Billing_Platform_Id__c), &apos;PCL#&apos;+Billing_Platform_Id__c, IF(!ISBLANK(PureCloud_OrgID__c), &apos;PCL#&apos;+PureCloud_OrgID__c, &apos;&apos;)), &apos;&apos;) +
if (TEXT(Cloud_Platform__c)==&apos;Premier Edition&apos;, IF(!ISBLANK(Billing_Platform_Id__c), &apos;Angel#&apos;+Billing_Platform_Id__c, IF(!ISBLANK(PE_Subscription_ID__c), &apos;Angel#&apos;+PE_Subscription_ID__c, &apos;&apos;)), &apos;&apos;) +
if (TEXT(Cloud_Platform__c)==&apos;Outbound Engagement&apos;, IF(!ISBLANK(Billing_Platform_Id__c), &apos;Portico#&apos;+Billing_Platform_Id__c, IF(!ISBLANK(Portico_Platform_Id__c), &apos;Portico#&apos;+Portico_Platform_Id__c, &apos;&apos;)), &apos;&apos;) +
if (TEXT(Cloud_Platform__c)==&apos;PureConnect Cloud&apos;, IF(!ISBLANK(Billing_Platform_Id__c), &apos;PCC#&apos;+Billing_Platform_Id__c, IF(!ISBLANK(PureConnect_Id__c), &apos;PCC#&apos;+PureConnect_Id__c, &apos;&apos;)), &apos;&apos;)</formula>
        <label>Billing Platform Id Hash</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
