    <validationRules>
        <fullName>Commitment_stage_rule</fullName>
        <active>true</active>
        <description>Commitment stage can be updated to accepted only by certain profiles</description>
        <errorConditionFormula>AND(
$Profile.Name &lt;&gt; &apos;PM (All)&apos;,
$Profile.Name &lt;&gt; &apos;Product Management View All &apos;,
$Profile.Name &lt;&gt; &apos;System Administrator&apos;,
$Profile.Name &lt;&gt; &apos;Data Administrator&apos;,
$Profile.Name &lt;&gt; &apos;Product Manager&apos;,
ISPICKVAL( Commitment_Progress__c  , &apos;Accepted&apos;)
)</errorConditionFormula>
        <errorMessage>&apos;You are not permitted to change Commitment stage to Accepted&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Restrict_Commitment_Stage_Update</fullName>
        <active>true</active>
        <description>Restrict_Commitment_Stage_Update_To_Lost_Opportunity</description>
        <errorConditionFormula>AND(
ISCHANGED(Commitment_Progress__c),(TEXT(Commitment_Progress__c)=&apos;Lost Opportunity&apos;),
NOT(OR(
CONTAINS($Profile.Name, &apos;System Administrator&apos;),
CONTAINS($Profile.Name, &apos;Data Administrator&apos;),
$User.BypassValidationRules__c
)))</errorConditionFormula>
        <errorMessage>Users can&apos;t update Commitment Stage to Lost Opportunity. Please select any other value.</errorMessage>
    </validationRules>
