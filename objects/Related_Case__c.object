        <fullName>Cannot_Change_Parent_Case</fullName>
        <active>true</active>
        <errorConditionFormula>ISCHANGED( Case__c)&amp;&amp; $User.BypassValidationRules__c = false</errorConditionFormula>
        <errorDisplayField>Parent_Case__c</errorDisplayField>
        <errorMessage>You cannot change the parent case field.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_Change_Parent_Related_Phase</fullName>
        <active>true</active>
        <errorConditionFormula>ISCHANGED(Related_Phase__c ) &amp;&amp; $User.BypassValidationRules__c = false</errorConditionFormula>
        <errorDisplayField>Parent_Case__c</errorDisplayField>
        <errorMessage>You cannot change the parent Related Phase field.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Prevent_Linkage_to_Same_Case</fullName>
        <active>true</active>
        <description>You cannot relate a case to itself</description>
        <errorConditionFormula>AND(
NOT(ISBLANK(Parent_Case__c)),
Case__c =  Parent_Case__c,
$User.BypassValidationRules__c = false
)</errorConditionFormula>
        <errorMessage>You cannot relate a case to itself</errorMessage>
    </validationRules>
