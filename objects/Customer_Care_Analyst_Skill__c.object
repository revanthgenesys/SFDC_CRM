        <label>Siebel ID</label>
        <length>15</length>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>true</unique>
    </fields>
    <fields>
        <fullName>Skill_Level__c</fullName>
        <description>This field will track the agent&apos;s skill level for this product</description>
        <externalId>false</externalId>
        <inlineHelpText>This field will track the agent&apos;s skill level for this product</inlineHelpText>
        <label>Skill Level</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Picklist</type>
        <valueSet>
            <valueSetDefinition>
                <sorted>false</sorted>
                <value>
                    <fullName>Expert</fullName>
                    <default>false</default>
                    <label>Expert</label>
                </value>
                <value>
                    <fullName>Intermediate</fullName>
                    <default>false</default>
                    <label>Intermediate</label>
                </value>
                <value>
                    <fullName>Novice</fullName>
                    <default>false</default>
                    <label>Novice</label>
                </value>
                <value>
                    <fullName>None</fullName>
                    <default>false</default>
                    <label>None</label>
                </value>
            </valueSetDefinition>
        </valueSet>
    </fields>
    <fields>
        <fullName>Skill_Score__c</fullName>
        <externalId>false</externalId>
        <formula>CASE( Skill_Level__c,
&quot;None&quot;,2.5, 
&quot;Novice&quot;,2,
&quot;Intermediate&quot;,1.5,
1)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Skill Score</label>
        <precision>18</precision>
        <required>false</required>
        <scale>1</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Support_Product__c</fullName>
        <deleteConstraint>SetNull</deleteConstraint>
