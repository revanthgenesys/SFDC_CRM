    <validationRules>
        <fullName>SLT_Check_For_0_Values</fullName>
        <active>true</active>
        <errorConditionFormula>IF (
   OR (
       Resolution_Critical__c =0,
        Resolution_High__c =0,
        Resolution_Low__c =0,
        Resolution_Medium__c =0,
        Resolution_Production_Down__c =0,
        Response_Critical__c =0,
         Response_High__c =0,
         Response_Low__c =0,
        Response_Medium__c =0,
        Response_Production_Down__c =0
   ), true, false
)</errorConditionFormula>
        <errorMessage>SL Target cannot be equal to 0. Please enter value(s) greater than 0.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SLT_STEU_Check_For_Blank_Values</fullName>
        <active>true</active>
        <errorConditionFormula>IF (
  AND(ISBLANK(Source_Template__c),
       RecordType.Name =&quot;SLT STEU&quot;,
    OR( ISNULL(Resolution_Critical__c),
        ISNULL(Resolution_High__c),
        ISNULL(Resolution_Low__c),
        ISNULL(Resolution_Medium__c),
        ISNULL(Resolution_Production_Down__c),
        ISNULL(Response_Critical__c),
        ISNULL(Response_High__c),
        ISNULL(Response_Low__c),
        ISNULL(Response_Medium__c),
        ISNULL(Response_Production_Down__c)
      )
   ), true, false
)</errorConditionFormula>
        <errorMessage>Please enter all SL Targets or select a Source Template to copy the Targets from.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SLT_ST_EU_required</fullName>
        <active>true</active>
        <description>Sold To and End User required for non-templates</description>
        <errorConditionFormula>IF( 
AND (RecordType.Name = &quot;SLT STEU&quot;,
    OR ( ISBLANK(Sold_To__c),
         ISBLANK(End_User__c) ) 
    ), true, false
)</errorConditionFormula>
        <errorDisplayField>End_User__c</errorDisplayField>
        <errorMessage>Please enter End User and Sold To accounts.</errorMessage>
    </validationRules>
