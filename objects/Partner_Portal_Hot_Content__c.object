<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>List</customSettingsType>
    <description>Used to enter the links displayed in the Hot Content section of the Partner Portal Home Page.</description>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>Content_Id__c</fullName>
        <description>The identifier of the linked content record.</description>
        <externalId>false</externalId>
        <label>Content Id</label>
        <length>64</length>
        <required>true</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Order__c</fullName>
        <description>Numeric value determining the order in which content links are displayed.  It sorts from lowest to highest.</description>
        <externalId>false</externalId>
        <label>Order</label>
        <precision>7</precision>
        <required>false</required>
        <scale>3</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <label>Partner Portal Hot Content</label>
    <visibility>Protected</visibility>
</CustomObject>
