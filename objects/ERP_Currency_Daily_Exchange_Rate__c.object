    <validationRules>
        <fullName>Appropriate_Rate_Type</fullName>
        <active>true</active>
        <errorConditionFormula>AND 
(  !$User.BypassValidationRules__c,
OR( 
TEXT(Exchange_Rate_Source__c) = &apos;User&apos;, 
TEXT(Exchange_Rate_Source__c) = &apos;Bloomberg&apos; 
), 
OR( 
TEXT(EXCHANGE_RATE_TYPE__c) = &apos;DAILY&apos;, 
TEXT(EXCHANGE_RATE_TYPE__c) = &apos;AVERAGE&apos;, 
TEXT(EXCHANGE_RATE_TYPE__c) = &apos;MONTHLY&apos; 
) 
)</errorConditionFormula>
        <errorDisplayField>EXCHANGE_RATE_TYPE__c</errorDisplayField>
        <errorMessage>This Rate Type can&apos;t be selected if Source is User or Bloomberg!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_From_Currency_Name</fullName>
        <active>true</active>
        <errorConditionFormula>AND( !$User.BypassValidationRules__c, 
NOT(ISNEW()),
ISPICKVAL(Exchange_Rate_Source__c, &apos;Xignite&apos;),

ISCHANGED(From_Currency_Name__c),

NOT(ISCHANGED(FROM_CURRENCY_CODE__c))  

)</errorConditionFormula>
        <errorDisplayField>From_Currency_Name__c</errorDisplayField>
        <errorMessage>Currency can&apos;t be changed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Source_value</fullName>
        <active>true</active>
        <errorConditionFormula>AND( !$User.BypassValidationRules__c,
ISPICKVAL(PRIORVALUE
(Exchange_Rate_Source__c), 
&apos;Xignite&apos;),
NOT(ISPICKVAL(Exchange_Rate_Source__c, &apos;Xignite&apos;))

)</errorConditionFormula>
        <errorDisplayField>Exchange_Rate_Source__c</errorDisplayField>
        <errorMessage>Source value can&apos;t be changed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_To_Currency_Name</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
 !$User.BypassValidationRules__c,
ISPICKVAL(Exchange_Rate_Source__c, &apos;Xignite&apos;), 

ISCHANGED(To_Currency_Name__c),

NOT(ISCHANGED(TO_CURRENCY_CODE__c))  

)</errorConditionFormula>
        <errorDisplayField>To_Currency_Name__c</errorDisplayField>
        <errorMessage>Currency can&apos;t be changed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Conversion_Editable</fullName>
        <active>true</active>
        <errorConditionFormula>AND(  !$User.BypassValidationRules__c,
NOT(ISNEW()),ERP_Currency_Freeze__c , 
OR(

ISCHANGED( EXCHANGE_RATE_TYPE__c ), 
ISCHANGED( Effective_Date__c ), 
ISCHANGED( From_Currency_Name__c ), 
ISCHANGED( To_Currency_Name__c ), 
ISCHANGED( Status__c ), 
ISCHANGED( Exchange_Rate_Source__c ), 
ISCHANGED( Period_Month_Year__c ) 
))</errorConditionFormula>
        <errorMessage>Only Conversion Rate, Average Conversion Rate and Balance Sheet Conv Rate is editable.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Lock_if_Source_is_Xignite</fullName>
        <active>true</active>
        <errorConditionFormula>AND( !$User.BypassValidationRules__c, NOT(ISNEW()),TEXT( Exchange_Rate_Source__c ) = &apos;Xignite&apos;)</errorConditionFormula>
        <errorMessage>You are not allowed to make changes.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Stop_creating_Xignite_Record</fullName>
        <active>true</active>
        <errorConditionFormula>AND( !$User.BypassValidationRules__c,ISNEW(),TEXT( Exchange_Rate_Source__c) = &apos;Xignite&apos;, Name = &apos;.&apos;)</errorConditionFormula>
        <errorMessage>You can&apos;t create Currency Exchange Rate for Source Xignite.</errorMessage>
    </validationRules>
