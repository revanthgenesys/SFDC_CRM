    <validationRules>
        <fullName>Disposition_Fields_Locked_if_not_Staged</fullName>
        <active>true</active>
        <description>This validation rule prevents the product manager from editing the &quot;action&quot; and &quot;locked&quot; fields on a record.</description>
        <errorConditionFormula>Stage__c = FALSE
&amp;&amp;
(
(NOT(ISPICKVAL(Action__c,&quot;&quot;)))
||
(Locked__c = TRUE)
)
&amp;&amp;
$User.BypassValidationRules__c = FALSE</errorConditionFormula>
        <errorDisplayField>Action__c</errorDisplayField>
        <errorMessage>&quot;Action&quot; &amp; &quot;Locked&quot; Fields Are For the Disposition Process Only</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Finance_Review_error</fullName>
        <active>true</active>
        <errorConditionFormula>AND(Not($User.BypassValidationRules__c),ISPICKVAL(POB_Type__c, &quot;Hardware&quot;),ISBLANK(DCOGS_Account__c),ISBLANK(LT_DCOGS_Account__c),ISBLANK(Intercompany_Account__c),ISBLANK(COGS_Account__c) , AND($Profile.Name = &quot;GEN - Finance&quot;))</errorConditionFormula>
        <errorMessage>For POB Type = Hardware please fill DCOGS Account,COGS Account, LTDCOGS Account and Internal Company Account.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Item_Name_Cannot_Be_Updated</fullName>
        <active>true</active>
        <description>Validation that once a product has been submitted to pricing, the item name cannot be updated.</description>
        <errorConditionFormula>NOT(ISPICKVAL(Status__c,&apos;To Be Updated&apos;)) 
&amp;&amp; 
NOT(ISPICKVAL(Status__c,&apos;Draft&apos;)) 
&amp;&amp; 
NOT(ISPICKVAL(Availability__c,&apos;BOM Update&apos;)) 
&amp;&amp; 
NOT(ISNEW()) 
&amp;&amp; 
ISCHANGED(Name) 
&amp;&amp; 
$User.BypassValidationRules__c = FALSE</errorConditionFormula>
        <errorDisplayField>Name</errorDisplayField>
        <errorMessage>Product Name Cannot Be Updated</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Item_Name_Length_60</fullName>
        <active>true</active>
        <description>Item Name length cannot exceed 60 characters as Netsuite cannot handle it</description>
        <errorConditionFormula>LEN(Name) &gt; 60 
&amp;&amp;  
Not($User.BypassValidationRules__c)
&amp;&amp;
$User.Alias != &apos;CindyH&apos;</errorConditionFormula>
        <errorDisplayField>Name</errorDisplayField>
        <errorMessage>The Name cannot exceed 60 characters</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>New_Version_is_needed_to_stage</fullName>
        <active>true</active>
        <errorConditionFormula>IF(AND(Stage__c,  ISBLANK(New_Family__c)), true, false)</errorConditionFormula>
        <errorMessage>Please select new family before staging an item.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Product_Manager_Field_Permissions</fullName>
        <active>false</active>
        <description>Validation that once a product has been submitted to pricing, the prooduc</description>
        <errorConditionFormula>$Profile.Name = &apos;Product Manager&apos;
&amp;&amp;
