    <validationRules>
        <fullName>Committed_Term_Date_Validation</fullName>
        <active>false</active>
        <description>Validation Rule to ensure that Committed Term Date Validation and Subscription Term Start Date  are the same for a CCDB\Subscription</description>
        <errorConditionFormula>and ( NOT($User.BypassValidationRules__c), Committed_Term_Date_Validation__c &lt;&gt; Subscription_Start_Date__c 
)</errorConditionFormula>
        <errorDisplayField>Committed_Term_Date_Validation__c</errorDisplayField>
        <errorMessage>Committed Term Date Validation and Subscription Term Start Date must be same.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Prevention_of_setting_of_future_date</fullName>
        <active>true</active>
        <description>Should not allow users to choose a future date for Subscription Term Start Date field.</description>
        <errorConditionFormula>AND( $Profile.Name &lt;&gt; &quot;System Administrator&quot;,ISPICKVAL(Offering_Type__c, &quot;PureCloud&quot;) , ISCHANGED(Start_Date__c), NOT(ISNEW()), Start_Date__c &gt; TODAY())</errorConditionFormula>
        <errorDisplayField>Start_Date__c</errorDisplayField>
        <errorMessage>Please choose a date earlier than today for Start Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validation_for_Ramp_End_Date</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
 NOT($User.BypassValidationRules__c),
 NOT(ISNEW()), 
 Ramp_End_Date__c &lt; Start_Date__c
)</errorConditionFormula>
        <errorDisplayField>Ramp_End_Date__c</errorDisplayField>
        <errorMessage>Please choose Ramp End Date later than Start Date. If there is no ramp period, Ramp End Date and Start Date can be same.</errorMessage>
    </validationRules>
