        <masterLabel>Create Renewal Opportunity</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/30.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/30.0/apex.js&quot;)} 
var BillingCategory = &quot;{!Subscription__c.Billing_Category__c}&quot;; 
var objSubId = &quot;{!Subscription__c.Id}&quot;; 
var Status = &quot;{!Subscription__c.Status__c }&quot;; 
var OfferingType = &quot;{!Subscription__c.Offering_Type__c}&quot; 
var userProfile = &quot;{!$Profile.Name}&quot;; 
var staticLabel = &apos;{!$Label.Profiles_to_create_M2M_Renewals}&apos;; 
if (BillingCategory == &apos;M2M&apos; &amp;&amp; Status == &apos;Active&apos; &amp;&amp; staticLabel.includes(userProfile) &amp;&amp; (OfferingType ==&apos;PureCloud&apos; || OfferingType ==&apos;PureConnect Cloud&apos;)) { 
try { 
var r = confirm(&quot;Do you wish to continue? If yes, please be patient as creation of opportunity will take a few seconds and and then will open in new tab.&quot;); 
if(r==true){ 
var oppId = sforce.apex.execute(&quot;CreateRenewalOppAndQuoteforM2MSubs&quot;, &quot;createRenewalOppandQuote&quot;, { 
objSubId: objSubId 
}); 
if(oppId[0] != null){ 
if (oppId[0].length == &apos;18&apos;) { 
window.open(&apos;/&apos; + oppId, &apos;_blank&apos;); 
} else { 
alert(oppId); 
} 
} 
else{ 
alert(&quot;Churned;Migrated; Status that prevent Renewal&quot;); 
} 
} 
} catch (err) { 
alert(err); 
} 

} else { 
if(!staticLabel.includes(userProfile)) 
{alert(&quot;Insufficient Privileges.\nYou do not have the level of access necessary to perform the operation you requested. Please contact your administrator if access is necessary.&quot;);} 
else 
{alert(&quot;This CCDB Subscription record cannot generate a renewal opportunity.\nPlease ensure all following criteria are met to successfully create a manual renewal opportunity:\nCCDB Subscription Status: Active\nOffering Type: PureCloud or PureConnect Cloud\nBilling Category: M2M&quot;);} 
}</url>
