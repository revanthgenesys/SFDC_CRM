    <webLinks>
        <fullName>Activate_Selected_SA</fullName>
        <availability>online</availability>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Activate Selected SA</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>true</requireRowSelection>
        <url>{!REQUIRESCRIPT(&apos;/soap/ajax/29.0/connection.js&apos;)}
var selectedRecords = {!GETRECORDIDS($ObjectType.Support_Access_Cloud__c)};
var updRecords = [];

if (selectedRecords[0] == null) {
alert(&quot;Please select at least one Support Access to update.&quot;)
}
else {
var recNum = selectedRecords.length;
if (confirm(&quot;This operation will Activate &quot; + recNum + &quot; records. Please confirm.&quot;)) {
for (var i=0, n=0; i &lt; recNum; i++, n++) {
var sa = new sforce.SObject(&quot;Support_Access_Cloud__c&quot;);
sa.id = selectedRecords[i];
sa.Access_Status__c = &apos;Active&apos;;
updRecords.push(sa);

if ( n == 45 || i == recNum-1 ) {
result = sforce.connection.update(updRecords);
if ( !result[0].getBoolean( &quot;success&quot; ) ) {
alert ( result[0].errors.message );
break;
}
updRecords = [];
n=0;
}
}
window.location.reload();
}
}</url>
