    <validationRules>
        <fullName>BRO</fullName>
        <active>false</active>
        <errorConditionFormula>AND( 

ISPICKVAL( NRO_Type__c, &apos;Entitlement Adjustment&apos;), 
AND(NOT($User.Can_Edit_NRO__c)),
AND(
OR(NOT(Contains($Profile.Name ,&apos;Finance&apos;)),NOT(Contains($Profile.Name ,&apos;GEN - Genesys Care Compliance ops&apos;)))
),
$User.BypassValidationRules__c=false 

)</errorConditionFormula>
        <errorDisplayField>of_Maint_Days__c</errorDisplayField>
        <errorMessage>Only Licensing Team  has access rights to do NRO of type Entitlement 
Adjustment.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_Update_Status_Order_Submitted</fullName>
        <active>false</active>
        <description>If Not Sys Admin or Gen Finance, then they Cannot Update an already Submitted Order //$Profile.Name!=&apos;System Administrator&apos;,</description>
        <errorConditionFormula>AND(Not(ISNEW()), AND( text(priorvalue(status__c))!= &apos;&apos;, text(priorvalue(status__c))!= &apos;Pending PM Approval&apos;) ,  RecordType.Name ==&apos;Non-Revenue Orders&apos;,  
AND($Profile.Name!=&apos;GEN - Finance&apos;,$Profile.Name!=&apos;System Administrator&apos;),$User.BypassValidationRules__c=false )</errorConditionFormula>
        <errorMessage>Please note once Order is Submitted, it cannot be Edited.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Make_Mandatory_for_Rebill_Fields</fullName>
        <active>true</active>
        <description>Rebill Order checkbox is Checked 
Please fill &quot;Netsuite Order Being Rebilled and Netsuite Invoice Being Rebilled&quot; on your Order Page</description>
        <errorConditionFormula>AND( Rebill_Order__c = true, OR( Netsuite_Order_Being_Rebilled__c = &quot;&quot; , Netsuite_Invoice_Being_Rebilled__c = &quot;&quot;) )</errorConditionFormula>
        <errorMessage>Rebill Order checkbox is Checked 
Please fill &quot;Netsuite Order Being Rebilled and Netsuite Invoice Being Rebilled&quot; on your Order Page</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Making_Subsidary_required_field</fullName>
        <active>true</active>
        <errorConditionFormula>And(IsPickval(status__c,&apos;Send to ERP&apos;),Isblank(Subsidaries__c), 
$User.BypassValidationRules__c=false )</errorConditionFormula>
        <errorDisplayField>Subsidaries__c</errorDisplayField>
        <errorMessage>Please enter the Subsidary before processing order for ERP.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Opportunity_Manditory_for_PPU_NRO_Orders</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISBLANK(Opportunity__c),RecordType.Name ==&apos;Non-Revenue Orders&apos;,ISPICKVAL( NRO_Type__c , &quot;Pay Per Use&quot; ), $User.BypassValidationRules__c=false)</errorConditionFormula>
        <errorMessage>A PPU NRO Requires a lookup to the Parent Opportunity, Please input this Opportunity below.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Order_Rejection_Reasoning_Needed</fullName>
        <active>true</active>
        <description>Order Rejection Reasons are Needed</description>
        <errorConditionFormula>AND(ISPICKVAL(status__c, &apos;Rejected&apos;) , LEN( TEXT(Reject_Reasons__c))=0, $User.BypassValidationRules__c=false )</errorConditionFormula>
        <errorDisplayField>Reject_Reasons__c</errorDisplayField>
        <errorMessage>Reject Reasons are Required when Order is Rejected</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Order_Type</fullName>
        <active>false</active>
        <errorConditionFormula>if( AND(Begins(Name,&apos;Order 201&apos;),Order_Type__c ==&apos;&apos;, Not(ISNEW())),true,false)</errorConditionFormula>
        <errorMessage>Order Type cannot be blank.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PS_Stop_Approved_if_Oppty_unapproved</fullName>
        <active>true</active>
        <description>What it does? Prevents changing the PS Order Status to Approved if the related PS Opportunity has not had the finance approved status set
Downstream Impact? Affects sending to ERP for processing
Owner? OM/IT</description>
        <errorConditionFormula>AND(
ISCHANGED(status__c),
RecordType.Name = &apos;PS Orders&apos;,
ISPICKVAL(status__c,&apos;Approved&apos;),
NOT(Finance_Oppty_Approval_Status__c = &apos;Approved&apos;),
NOT($User.BypassValidationRules__c)
)</errorConditionFormula>
        <errorMessage>The associated PS Opportunity has not been approved by the OM team, so this Order cannot be marked as Approved at this time. Please update the related opportunity approval before proceeding</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RevPro_PS_Order_Check_OLI_to_Oppty_Prod</fullName>
        <active>true</active>
        <description>Checks if an order line item on a PS Order has the needed association to back to the opportunity products, and if missing will stop the order being approved</description>
        <errorConditionFormula>AND( 
$User.BypassValidationRules__c=false,
ISPICKVAL(License_FYM_detail__c, &apos;PS Orders&apos;), 
ISCHANGED(status__c), 
ISPICKVAL(status__c,&apos;Approved&apos;),
PS_Order_Lines_without_Opp_Prod_Link__c  &gt; 0
)</errorConditionFormula>
        <errorMessage>Order cannot be approved as the link back to a PS Opportunity Product on an Order Line item is not present (Required for RevPro)</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RevPro_PS_Order_POB_Mismatch_Check</fullName>
        <active>true</active>
        <description>Checks if an order line item POB Type on a PS Order does not have a corresponding match back to the PS opportunity product POB Types</description>
        <errorConditionFormula>AND( 
$User.BypassValidationRules__c=false,
ISPICKVAL(License_FYM_detail__c, &apos;PS Orders&apos;), 
ISCHANGED(status__c), 
ISPICKVAL(status__c,&apos;Approved&apos;),
Order_Lines_with_POB_Mismatch__c 
)</errorConditionFormula>
        <errorMessage>PS Order cannot be approved as the Order Line POB Type is not matched to a PS Opportunity Product POB Type (Required for RevPro)</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Revpro_Cloud_approved_status_Line_Number</fullName>
        <active>true</active>
        <description>Order cannot be approved if Line Numner is not populated on any line</description>
        <errorConditionFormula>AND( 
$User.BypassValidationRules__c=false , 
ISCHANGED(status__c), 
ISPICKVAL(status__c,&apos;Approved&apos;),
Order_Lines_Without_Line_Numbers__c &gt; 0,
(Order_RecordType_Name__c = &apos;Cloud Billing Master Order&apos; ),
OR( Quote__r.Order_Type__c == &apos;New Subscription&apos;, Quote__r.Order_Type__c== &apos;Renew Subscription&apos;,Quote__r.Order_Type__c == &apos;Replace Subscription&apos; ),
(DATEVALUE(Quote__r.CreatedDate) &gt; Datevalue(&quot;2019-07-01&quot;)
)
)</errorConditionFormula>
        <errorDisplayField>status__c</errorDisplayField>
        <errorMessage>Order cannot be approved if Line Numbers is not populated on order lines.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Revpro_Cloud_approved_status_POB</fullName>
        <active>true</active>
        <description>Order cannot be approved if POB Type is not populated on any line</description>
        <errorConditionFormula>AND(
$User.BypassValidationRules__c=false ,
ISCHANGED(status__c),
ISPICKVAL(status__c,&apos;Approved&apos;),
Order_Lines_Without_POB_Type__c &gt; 0,
(RecordTypeId = &apos;012U0000000MqMG&apos; || RecordTypeId =&apos;012U0000000kF07&apos; || RecordTypeId = &apos;012U0000000kGkO&apos; || RecordTypeId = &apos;012U0000000kGkT&apos; || RecordTypeId = &apos;0120B000000t7ui&apos; )
)</errorConditionFormula>
        <errorDisplayField>status__c</errorDisplayField>
        <errorMessage>Order cannot be approved if POB Type is not populated on order lines.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Revpro_Cloud_approved_status_Rev_Classif</fullName>
