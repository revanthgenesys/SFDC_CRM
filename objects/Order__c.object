        <externalId>false</externalId>
        <formula>IF(RecordTypeId &lt;&gt;&apos;012U0000000kF07&apos;, /* If Not Cloud Monthly Billing Order */ 
/*IF(TEXT(License_FYM_detail__c) &lt;&gt; &apos;PureConnect-GU&apos;, 
IF(TEXT(License_FYM_detail__c) &lt;&gt; &apos;PureEngage-GU&apos;,*/ 
IF(TEXT(License_FYM_detail__c) &lt;&gt; &apos;GU Order&apos;, 
IF(TEXT(License_FYM_detail__c) &lt;&gt; &apos;Rental Order&apos; , 
IF(TEXT(License_FYM_detail__c) &lt;&gt; &apos;PPU Order&apos; , 
IF(TEXT(License_FYM_detail__c) &lt;&gt; &apos;Hardware Order&apos; , 
IF(TEXT(License_FYM_detail__c) &lt;&gt; &apos;Subscription Order&apos; , 
IF(TEXT(License_FYM_detail__c) &lt;&gt; &apos;PS Orders&apos; , 
IF(Opportunity_Type__c &lt;&gt; &apos;Genesys_Renewal&apos;, 
IF(ISBLANK(Quote__c), 
IF(Zuora_Quote__c &lt;&gt; &apos;&apos;, &apos;PureCloud-Cloud Order&apos;, &apos;&apos;), 
IF(Quote__r.Offering_Type__c == &apos;PureCloud&apos;, 
&apos;PureCloud-Cloud Order&apos;, 
IF(Quote__r.Offering_Type__c == &apos;PureConnect Cloud&apos;, 
&apos;PureConnect-Cloud Order&apos;, 
IF(Quote__r.Offering_Type__c == &apos;PureEngage Cloud&apos;, 
&apos;PureEngage-Cloud Order&apos;, 
IF(Quote__r.Offering_Type__c == &apos;Business Edition&apos;, 
&apos;PureEngage-Cloud Order&apos;, 
(IF(Quote__r.Offering_Type__c == &apos;PureConnect On Premise&apos;, 
&apos;PureConnect&apos;, 
&apos;PureEngage&apos;) + &apos; - &apos; + TEXT(Quote__r.Quote_Business_Type__c))))))), 
IF(ISBLANK(Quote__c),&apos;&apos;, 
IF(OR(Quote__r.Service_Level__c == &apos;Standard Care&apos;,Quote__r.Service_Level__c == &apos;Basic Care&apos;,Quote__r.Service_Level__c == &apos;Priority Care&apos;,Quote__r.Service_Level__c == &apos;VIP Priority Care&apos;,Quote__r.Service_Level__c == &apos;Latitude Care&apos;), 
&apos;PureConnect&apos;, &apos;PureEngage&apos;) + &apos; - Renewal&apos;)), 
PS_Line_of_Business__c &amp; &quot; - PS Orders&quot;), 

Quote__r.Offering_Type__c), 
If(Quote__r.Offering_Type__c==&apos;&apos;,&apos;PureEngage On Premise&apos;,Quote__r.Offering_Type__c) + &apos; - Hardware&apos;), 
/*ppu usage order */ 
If(RecordTypeId &lt;&gt; &apos;012U0000000MGoM&apos;, TEXT(Opportunity__r.Lines_of_Businesses__c) + &apos; - PPU&apos;, TEXT(Opportunity__r.Lines_of_Businesses__c) + &apos; - PPU Billing&apos;) 

), 
If(Quote__r.Offering_Type__c==&apos;&apos;,&apos;PureEngage&apos;,Quote__r.Offering_Type__c) + &apos; - Rental&apos;), 
text(Opportunity__r.Lines_of_Businesses__c) + &apos; - GU&apos;), 
/*&apos;PureEngage - GU&apos;), 
&apos;PureConnect - GU&apos;),*/ 

/* Cloud Monthly Billing Order */ 
IF(CONTAINS(Sales_Org__c, &apos;Subscription&apos;), Sales_Org__c + &apos; - Burst&apos;, 
IF(ISBLANK(Sales_Org__c), Offering_Type__c, Sales_Org__c) + &apos; - Cloud Monthly Billing&apos;))</formula>
