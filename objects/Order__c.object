if ( (orderStatus != &quot;Send to ERP&quot; &amp;&amp; orderStatus != &quot;Completed&quot;) || groupNameList == null) {

var regex = new RegExp(&quot;;&quot;, &quot;g&quot;);
var groupNamesStr = groupNameList.replace(regex, &quot;&apos;,&apos;&quot;);

  var verifiedGroupNamesQuery = &quot;SELECT Name FROM Product_Group__c WHERE Name IN (&apos;&quot; + groupNamesStr + &quot;&apos;)&quot;;
  resultVerify = sforce.connection.query(verifiedGroupNamesQuery);  
  verifiedNames = resultVerify.getArray(&quot;records&quot;);
  var groupNameArray = groupNameList.split(&quot;;&quot;);
  var missingGroup = &quot;&quot;;
  for (var i=0; i &lt; groupNameArray.length; i++)   {
    if (groupNameArray[i] == &quot;&quot;)
	    continue;
    var found=false;
    for (var n=0; n &lt; verifiedNames.length; n++) {
      if (verifiedNames[n].Name == groupNameArray[i]) {
	       found=true;
		   break;
	   }
    }
	if (found == false)
	   missingGroup += &quot;\n&quot; + groupNameArray[i];
  }
  
  if (missingGroup != &quot;&quot;)
    alert(&quot;NOT FOUND Bundle(s): &quot; + missingGroup + &quot;\n Please notify Applications Support Group.&quot; );
     
  var groupToAddQuery = &quot;SELECT Product__c, Quantity__c, APN_Number__c FROM Product_Group_Item__c WHERE Product_Group__r.Name IN (&apos;&quot; + groupNamesStr + &quot;&apos;)&quot;;	
