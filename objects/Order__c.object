  var prodIdToAdd = [];
  var qntToAdd = [];
  var apnNum = [];
  var lenAdd = recordsToAdd.length;
  var lenExist = recordsExist.length;
 
 for (var i=0; i &lt; lenAdd; i++) { 
  var exist=false; 
  for (var n=0; n &lt; lenExist; n++)  {
     if ( recordsExist[n].Product__c == recordsToAdd[i].Product__c ) {
	    exist=true;
		break;
	 }
  }   
  if (prodIdToAdd.indexOf(recordsToAdd[i].Product__c) == -1 &amp;&amp; exist == false)
	  prodIdToAdd.push(recordsToAdd[i].Product__c);
	  qntToAdd.push(recordsToAdd[i].Quantity__c);
	  apnNum.push(recordsToAdd[i].APN_Number__c);
