      lineItemsAdd.push(lineItem);
  }
  if ( lineItemsAdd.length &gt; 0 ) {
   if (confirm(&quot;This operation will add &quot; + lineItemsAdd.length + &quot; new Line Items. Please confirm.&quot;)) { 
	  var addResult = sforce.connection.create(lineItemsAdd);
