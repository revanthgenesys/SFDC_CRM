        <url>{!REQUIRESCRIPT(&apos;/soap/ajax/27.0/connection.js&apos;)}  
{!REQUIRESCRIPT(&apos;/soap/ajax/21.0/apex.js&apos;)} 


a = sforce.apex.execute(&quot;dealSupportOpportunity&quot;,&quot;createopty&quot;,{tid:&quot;{!DSR_Opportunity_Detail__c.Id}&quot;});  


if(a == &apos;failed&apos;){ 
alert(&apos;Opportunity creation failed&apos;);  
}  

else{  
