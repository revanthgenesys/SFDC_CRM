    <webLinks>
        <fullName>Activate_FTP_Account</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Activate FTP Account</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&apos;/soap/ajax/29.0/connection.js&apos;)}

var reqStatus = &apos;{!Custom_SWDL_Request__c.Status__c}&apos;;
var usacc = &apos;{!Custom_SWDL_Request__c.FTP_User__c}&apos;;
alertstr = &quot;&quot;;

if (reqStatus == &quot;Closed&quot;) {
  alertstr = &quot;Request in status Closed cannot be modified.&quot;;
}
else if (usacc != &quot;&quot;) {
   alertstr = &quot;The FTP account cannot be modified.&quot;;
}

if ( reqStatus != &quot;Closed&quot; &amp;&amp; usacc == &quot;&quot; ) {

   var ftpAcct=&quot;&quot;, ftpPassw=&quot;&quot;; 
   var i=0, rand=0;
var PASSWDCHARS = &apos;_0123456789ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz&apos;;
	for (i=0; i &lt; 8; i++)  { 		
	   rand = Math.round( 61 * Math.random() );  
	   ftpPassw += PASSWDCHARS.substring( rand, rand+1);       			
  }	

var subsQuery = sforce.connection.query(&quot;SELECT Subsidary__c, Subsidary__r.Name FROM Account WHERE Id= &apos;{!Custom_SWDL_Request__c.End_User_AccountId__c}&apos;&quot;);
var records = subsQuery.getArray(&quot;records&quot;);
var subsName = &quot;&quot;;
if (records.length &gt;0 &amp;&amp; records[0].Subsidary__c != null) {
subsName = records[0].Subsidary__r.Name;
} 

var acctPref = &quot;cpe&quot;;
if ( subsName == &quot;8100&quot; )
acctPref = &quot;cpna&quot;; 

  ftpAcct = acctPref + &quot;0&quot; + Math.round(Math.random()*(9990-1000)+100);	

   var now = new Date();
   var date = [ now.getMonth() + 1, now.getDate(), now.getFullYear() ];
   var time = [ now.getHours(), now.getMinutes(), now.getSeconds() ];
   var timest = date.join(&quot;/&quot;) + &quot; &quot; + time.join(&quot;:&quot;);
   if ( time[1] &lt; 10 ) 
      time[1] = &quot;0&quot; + time[1];

sforce.connection.remoteFunction(
 {
    url :
	&quot;https://softftpus.genesys.com:8443/prjEnterprise/eftp/users/setuser?&quot; +
	&quot;u=&quot; + ftpAcct + &quot;&amp;&quot; + 
	&quot;p=&quot; + ftpPassw + &quot;&amp;&quot; + 
        &quot;d=PSAccount&quot; + &quot;&amp;&quot; +
	&quot;sf={!$User.Username}&quot;,

    onFailure : function(response) {
       alert (&quot;FTP Account activation failed: &quot; + response);
    },
    onSuccess : function(response) {

    if (response == &quot;Success&quot;) {
	
     var updateRecord = new Array(); 
     var reqQuery = &quot;SELECT Id, FTP_Comments__c FROM Custom_SWDL_Request__c WHERE Id =&apos;{!Custom_SWDL_Request__c.Id}&apos; limit 1&quot;;	

      result = sforce.connection.query(reqQuery);  
      records = result.getArray(&quot;records&quot;);

    if(records[0]) {
	var update_Req = records[0];
	update_Req.FTP_User__c = ftpAcct;
	update_Req.FTP_Password__c = ftpPassw;

        var comm = &quot;&quot;;
        if (update_Req.FTP_Comments__c != null)
              comm = update_Req.FTP_Comments__c;
	update_Req.FTP_Comments__c = &quot;[&quot; + timest  + &quot;] FTP Account &quot; + ftpAcct + 
                  &quot; created and will stay active for 20 days\n&quot; + comm;

	updateRecord.push(update_Req);
      }

      result = sforce.connection.update(updateRecord);
      if ( !result[0].getBoolean( &quot;success&quot; ) )
	 alert (result[0].errors.message);
      else
	 alert (&quot;The FTP account has been successfully activated.&quot;);		
		 parent.location.href = parent.location.href;   
      }
      else
        alert (&quot;FTP Account activation failed. Status message: &quot; + response);
    }
 });  

}
else {
     alert (alertstr);
}</url>
    </webLinks>
</CustomObject>
