    <allowInChatterGroups>false</allowInChatterGroups>
    <compactLayoutAssignment>SYSTEM</compactLayoutAssignment>
    <deploymentStatus>Deployed</deploymentStatus>
    <description>Details: Used for storing transactional PS project revenue information for feeding to the RevPro system. Data is written from the PS PureProject PSA tool and read into RevPro.
Owned by: Professional Services
Downstream Impact: Any changes may affect the PSA tool integration to salesforce and the RevPro integration</description>
    <enableActivities>false</enableActivities>
    <enableBulkApi>true</enableBulkApi>
    <enableFeeds>false</enableFeeds>
    <enableHistory>false</enableHistory>
    <enableLicensing>false</enableLicensing>
    <enableReports>false</enableReports>
    <enableSearch>true</enableSearch>
    <enableSharing>true</enableSharing>
    <enableStreamingApi>true</enableStreamingApi>
    <externalSharingModel>ControlledByParent</externalSharingModel>
    <fields>
        <fullName>Associated_Expert_Apps_Amount__c</fullName>
        <description>The amount of PS Expert Apps associated to this CAS Event</description>
        <externalId>false</externalId>
        <label>Associated Expert Apps Amount</label>
        <precision>16</precision>
        <required>false</required>
        <scale>2</scale>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>Associated_Opportunity_Product_ID__c</fullName>
        <description>The salesforce ID of the Opportunity Product item to which this PS Revenue Event relates. This is just the ID and not a lookup and will be used with the integration to RevPro to identify to which SO line this event is associated.</description>
        <externalId>false</externalId>
        <label>Associated Opportunity Product ID</label>
        <length>30</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Associated_Opportunity_Product_Line_Num__c</fullName>
        <description>What it Does? Shows the line number of the associated opportunity product for this revenue event
Downstream Impact? Used for tracking/reporting purposes
Owner? RevPro/IT</description>
        <externalId>false</externalId>
        <label>Associated Opportunity Product Line Num</label>
        <precision>4</precision>
        <required>false</required>
        <scale>0</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Associated_Opportunity_Product_Name__c</fullName>
        <description>The name of the opportunity product associated to this RevPro Event</description>
        <externalId>false</externalId>
        <label>Associated Opportunity Product Name</label>
        <length>250</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Associated_Opportunity_Product__c</fullName>
        <externalId>false</externalId>
        <formula>IF( ISBLANK(Associated_Opportunity_Product_ID__c) , 
&quot;&quot;, 
HYPERLINK(LEFT( $Api.Partner_Server_URL_440, FIND( &apos;/services&apos;, $Api.Partner_Server_URL_440)) &amp; Associated_Opportunity_Product_ID__c , Associated_Opportunity_Product_Name__c ) 
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Associated Opportunity Product</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>CAS_Term_End_Date__c</fullName>
        <description>The end Date for the CAS (Custom Application Support) term for a CAS Event (RevPro)</description>
        <externalId>false</externalId>
        <label>CAS Term End Date</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Date</type>
    </fields>
    <fields>
        <fullName>CAS_Term_Start_Date__c</fullName>
        <description>The start Date for the CAS (Custom Application Support) term for a CAS Event (RevPro)</description>
        <externalId>false</externalId>
        <label>CAS Term Start Date</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Date</type>
    </fields>
    <fields>
        <fullName>Master_Opportunity__c</fullName>
        <deleteConstraint>SetNull</deleteConstraint>
        <description>The reference to the master opportunity, which will either be the same as the opportunity master detail link, or if this record is related to a change request (CR/PCR), this will be the reference the parent project linked opportunity record</description>
        <externalId>false</externalId>
        <label>Master Opportunity</label>
        <referenceTo>Opportunity</referenceTo>
        <relationshipLabel>PS Revenue Details (Master Opportunity)</relationshipLabel>
        <relationshipName>PS_Revenue_Details_Master</relationshipName>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>Opportunity__c</fullName>
        <description>The Opportunity to which this record is related</description>
        <externalId>false</externalId>
        <label>Opportunity</label>
        <referenceTo>Opportunity</referenceTo>
        <relationshipLabel>PS Revenue Details</relationshipLabel>
        <relationshipName>PS_Revenue_Details</relationshipName>
        <relationshipOrder>0</relationshipOrder>
        <reparentableMasterDetail>false</reparentableMasterDetail>
        <trackTrending>false</trackTrending>
        <type>MasterDetail</type>
        <writeRequiresMasterRead>false</writeRequiresMasterRead>
    </fields>
    <fields>
        <fullName>PS_Revenue_Date__c</fullName>
        <description>The date for which this PS Revenue transaction record applies</description>
        <externalId>false</externalId>
        <label>PS Revenue Date</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Date</type>
    </fields>
    <fields>
        <fullName>Quantity__c</fullName>
        <description>The quantity associated to the revenue transaction record</description>
        <externalId>false</externalId>
        <label>Quantity</label>
        <precision>16</precision>
        <required>false</required>
        <scale>2</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Revenue_Type__c</fullName>
        <description>The type of the revenue transaction event for PS</description>
        <externalId>false</externalId>
        <label>Revenue Type</label>
        <required>true</required>
        <trackTrending>false</trackTrending>
        <type>Picklist</type>
        <valueSet>
            <restricted>true</restricted>
            <valueSetDefinition>
                <sorted>false</sorted>
                <value>
                    <fullName>Time</fullName>
                    <default>false</default>
                    <label>Time</label>
                </value>
                <value>
                    <fullName>Expense</fullName>
                    <default>false</default>
                    <label>Expense</label>
                </value>
                <value>
                    <fullName>Fixed Fee</fullName>
                    <default>false</default>
                    <label>Fixed Fee</label>
                </value>
                <value>
                    <fullName>Percent</fullName>
                    <default>false</default>
                    <label>Percent</label>
                </value>
                <value>
                    <fullName>Milestone</fullName>
                    <default>false</default>
                    <label>Milestone</label>
                </value>
            </valueSetDefinition>
        </valueSet>
    </fields>
    <fields>
        <fullName>Total_Amount__c</fullName>
        <description>The total amount of the PS Revenue transaction</description>
        <externalId>false</externalId>
        <label>Total Amount</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
    </fields>
    <label>PS Revenue Detail</label>
    <listViews>
        <fullName>All</fullName>
        <filterScope>Everything</filterScope>
        <label>All</label>
    </listViews>
    <nameField>
        <displayFormat>PSR-{00000}</displayFormat>
        <label>PS Revenue Detail Number</label>
        <type>AutoNumber</type>
    </nameField>
    <pluralLabel>PS Revenue Details</pluralLabel>
