    <validationRules>
        <fullName>Prevention_of_changing_fields</fullName>
        <active>true</active>
        <description>If GCR__c.Status__c is ‘10-Completed’, user cannot modify Opportunity, CCDB or Subscription Sub Type fields under GCR__c</description>
        <errorConditionFormula>AND( TEXT(Status__c) = &apos;10-Completed&apos;, OR(ISCHANGED( Opportunity__c ) ,ISCHANGED( CCDB_Subscription__c ), ISCHANGED( Subscription_Sub_Type__c )),TEXT(Offering_Type__c) = &apos;PureCloud&apos;)</errorConditionFormula>
        <errorMessage>Opportunity,CCDB/Subscription or Subscription Sub Type cannot be changed when status = &apos;10-Completed&apos;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Status_and_Attorney_name_validation</fullName>
        <active>true</active>
        <errorConditionFormula>AND( ISBLANK( TEXT(Attorney_Name__c) ) ,
OR( 
ISPICKVAL( Status__c , &quot;Executed&quot;),
ISPICKVAL( Status__c , &quot;Completed&quot;),
ISPICKVAL( Status__c , &quot;Approved&quot;)
)
)</errorConditionFormula>
        <errorMessage>Status can not be changed to Executed/Completed/Pending Approval/Approved when no attorney is assigned</errorMessage>
    </validationRules>
