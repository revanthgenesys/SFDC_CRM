    <fields>
        <fullName>Outsourced_Service_Duration__c</fullName>
        <externalId>false</externalId>
        <formula>IF(NOT(ISPICKVAL(Request_Type__c, &quot;Outsourced&quot;)), 0,

IF(ISBLANK(Completed_by_Vendor_Date__c),
((IF(Sent_to_Vendor_Date__c &lt;&gt; Today() ,
IF( /** Condition */
AND(
(5 - (CASE(MOD( Sent_to_Vendor_Date__c - DATE(1900, 1, 6), 7), 0, 0, 1, 5, 2, 5, 3, 4, 4, 3, 5, 2, 6, 1, 0)) &lt; (CASE(MOD( Today() - DATE(1900, 1, 6), 7), 0, 0, 1, 0, 2, 1, 3, 2, 4, 3, 5, 4, 6, 5, 0)) ),
((( Today() - Sent_to_Vendor_Date__c ) + 1) &lt; 7)
),
/** Value to YES */
(
(CASE(MOD( Today() - DATE(1900, 1, 6), 7), 0, 0, 1, 0, 2, 1, 3, 2, 4, 3, 5, 4, 6, 5, 0)) - (5 - (CASE(MOD( Sent_to_Vendor_Date__c - DATE(1900, 1, 6), 7), 0, 0, 1, 5, 2, 5, 3, 4, 4, 3, 5, 2, 6, 1, 0)))
),
/** Value to NOT */
(
((FLOOR((( Today() - Sent_to_Vendor_Date__c ) - (CASE(MOD( Sent_to_Vendor_Date__c - DATE(1900, 1, 6), 7), 0, 0, 1, 6, 2, 5, 3, 4, 4, 3, 5, 2, 6, 1, 0))) / 7)) * 5) +
(CASE(MOD( Sent_to_Vendor_Date__c - DATE(1900, 1, 6), 7), 0, 0, 1, 5, 2, 5, 3, 4, 4, 3, 5, 2, 6, 1, 0)) +
(CASE(MOD( Today() - DATE(1900, 1, 6), 7), 0, 0, 1, 0, 2, 1, 3, 2, 4, 3, 5, 4, 6, 5, 0))
)
)
,0)) - 1)
,

(IF(Sent_to_Vendor_Date__c &lt;&gt; Completed_by_Vendor_Date__c ,
IF( /** Condition */
AND(
(5 - (CASE(MOD( Sent_to_Vendor_Date__c - DATE(1900, 1, 6), 7), 0, 0, 1, 5, 2, 5, 3, 4, 4, 3, 5, 2, 6, 1, 0)) &lt; (CASE(MOD( Completed_by_Vendor_Date__c - DATE(1900, 1, 6), 7), 0, 0, 1, 0, 2, 1, 3, 2, 4, 3, 5, 4, 6, 5, 0)) ),
((( Completed_by_Vendor_Date__c - Sent_to_Vendor_Date__c ) + 1) &lt; 7)
),
/** Value to YES */
(
(CASE(MOD( Completed_by_Vendor_Date__c - DATE(1900, 1, 6), 7), 0, 0, 1, 0, 2, 1, 3, 2, 4, 3, 5, 4, 6, 5, 0)) - (5 - (CASE(MOD( Sent_to_Vendor_Date__c - DATE(1900, 1, 6), 7), 0, 0, 1, 5, 2, 5, 3, 4, 4, 3, 5, 2, 6, 1, 0)))
),
/** Value to NOT */
(
((FLOOR((( Completed_by_Vendor_Date__c - Sent_to_Vendor_Date__c ) - (CASE(MOD( Sent_to_Vendor_Date__c - DATE(1900, 1, 6), 7), 0, 0, 1, 6, 2, 5, 3, 4, 4, 3, 5, 2, 6, 1, 0))) / 7)) * 5) +
(CASE(MOD( Sent_to_Vendor_Date__c - DATE(1900, 1, 6), 7), 0, 0, 1, 5, 2, 5, 3, 4, 4, 3, 5, 2, 6, 1, 0)) +
(CASE(MOD( Completed_by_Vendor_Date__c - DATE(1900, 1, 6), 7), 0, 0, 1, 0, 2, 1, 3, 2, 4, 3, 5, 4, 6, 5, 0))
)
)
,0)) - 1))</formula>
        <label>Outsourced Service Duration</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
