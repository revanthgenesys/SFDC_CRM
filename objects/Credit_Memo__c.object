    <validationRules>
        <fullName>Approved_Pending_CreditMemo_Status_Rule</fullName>
        <active>false</active>
        <errorConditionFormula>AND( !$User.BypassValidationRules__c, ISCHANGED(Status__c) , ISPICKVAL( Status__c , &apos;Approved - Pending CreditMemo&apos;) , NOT(ISPICKVAL(PRIORVALUE( Status__c ),&apos;Approved - Pending Debooking&apos;)))</errorConditionFormula>
        <errorMessage>&apos;Approved - Pending CreditMemo&apos; status can be set only after &quot;Approved - Pending Debooking&quot;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Awaiting_Rebill_Status_Rule</fullName>
        <active>false</active>
        <errorConditionFormula>AND( !$User.BypassValidationRules__c, ISCHANGED(Status__c) , ISPICKVAL( Status__c , &apos;Awaiting Rebill&apos;) , NOT(ISPICKVAL(PRIORVALUE( Status__c ),&apos;Approved - Pending CreditMemo&apos;)))</errorConditionFormula>
        <errorMessage>&apos;Awaiting Rebill&apos; status can be set only after &quot;Approved - Pending CreditMemo&quot;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Completed_Status_Rule</fullName>
        <active>false</active>
        <description>This status can be set only after &quot;Awaiting Rebill&quot;.</description>
        <errorConditionFormula>AND( !$User.BypassValidationRules__c, ISPICKVAL( Status__c , &apos;Completed&apos;) , NOT( ISPICKVAL( PRIORVALUE(Status__c ), 
&apos;Awaiting Rebill&apos;)), NOT( ISPICKVAL( PRIORVALUE(Status__c ), 
&apos;Cancelled&apos;)), NOT( ISPICKVAL( PRIORVALUE(Status__c ), 
&apos;Completed&apos;)))</errorConditionFormula>
        <errorMessage>&apos;Completed&apos; status can be set only after &quot;Awaiting Rebill&quot;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>More_Info_Reason_Required</fullName>
        <active>true</active>
        <description>If Status is More Info Needed then Reason is required</description>
        <errorConditionFormula>AND( !$User.BypassValidationRules__c, TEXT(Status__c) = &apos;More Info Needed&apos;,ISBLANK( More_Info_Reason__c ))</errorConditionFormula>
        <errorDisplayField>More_Info_Reason__c</errorDisplayField>
        <errorMessage>More Info Reason is required if Status is More Info Needed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>NS_CM_Number_Required_Rule</fullName>
        <active>true</active>
        <errorConditionFormula>AND( !$User.BypassValidationRules__c, ISCHANGED(Status__c) , ISPICKVAL(Status__c, &apos;Completed&apos;) , ISBLANK( NS_CM_Number__c) )</errorConditionFormula>
        <errorDisplayField>NS_CM_Number__c</errorDisplayField>
        <errorMessage>Please enter NS CM number.  Status cannot be changed to Completed without NS CM Number.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>NS_Debook_Number_Required_Rule</fullName>
        <active>false</active>
        <errorConditionFormula>AND( !$User.BypassValidationRules__c, ISCHANGED(Status__c) , ISPICKVAL(Status__c, &apos;Approved - Pending CreditMemo&apos;) ,  ISBLANK( NS_Debook_Number__c ) )</errorConditionFormula>
        <errorDisplayField>NS_Debook_Number__c</errorDisplayField>
        <errorMessage>Please enter NS Debooking number.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Rebill_Sales_Order_as_mandatory</fullName>
        <active>true</active>
        <errorConditionFormula>AND( !$User.BypassValidationRules__c, ISPICKVAL(Credit_Type__c ,&apos;Credit/Rebill&apos;), 
ISPICKVAL(Status__c, &apos;Completed&apos;),OR(ISBLANK(Rebill_Amount_NU__c),ISBLANK(Rebill_Invoice__c),ISBLANK(Rebill_Sales_Order__c)))</errorConditionFormula>
        <errorMessage>Rebill Sales Order &amp; Rebill Invoice# and Rebill Amount is mandatory for the Credit type as Credit/Rebill</errorMessage>
    </validationRules>
