    <startsWith>Vowel</startsWith>
    <validationRules>
        <fullName>Exactly_one_expense_object</fullName>
        <active>true</active>
        <description>Please associate this expense to exactly one object.</description>
        <errorConditionFormula>IF( ISBLANK( Marketing_Source__c ) , 0, 1) +
 IF( ISBLANK( Marketing_Program__c ) , 0, 1) +
 IF( ISBLANK( Campaign_Asset__c ) , 0, 1) +
 IF( ISBLANK( Campaign__c ) , 0, 1) +
 IF( AND( ISBLANK( External_Expense_Object__c ),  ISBLANK( External_Expense_Object_Record_Id__c )), 0, 1) &lt;&gt; 1</errorConditionFormula>
        <errorMessage>Please associate this expense to exactly one object.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>External_Id_if_External_expense_object</fullName>
        <active>true</active>
        <errorConditionFormula>OR(
AND(NOT(ISBLANK( External_Expense_Object__c )), ISBLANK( External_Expense_Object_Record_Id__c )),
AND(ISBLANK( External_Expense_Object__c ), NOT(ISBLANK( External_Expense_Object_Record_Id__c )))
)</errorConditionFormula>
        <errorMessage>Please include an External Expense Object Record Id for External Object expenses.</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>
