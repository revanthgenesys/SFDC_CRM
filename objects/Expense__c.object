    <fields>
        <fullName>Amount_Type__c</fullName>
        <description>This field determines how the actual cost is calculated from the amount field.</description>
        <externalId>false</externalId>
        <inlineHelpText>This field determines how the actual cost is calculated from the amount field.</inlineHelpText>
        <label>Amount Type</label>
        <required>true</required>
