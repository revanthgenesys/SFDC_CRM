        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Send Invoice</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/33.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/33.0/apex.js&quot;)} 

var opt = confirm(&quot;Are you sure you want to email the invoice ?&quot;);
if (opt == true) 
{
	var currentStatus = &apos;{!Invoice__c.Status__c}&apos;;
	if(currentStatus == &apos;Email Sent&apos;)
	{
		alert(&apos;Invoice has been sent out already. Please clear the Invoice status to send again.&apos;);
	}	
	else
	{
		var objInvoice = new sforce.SObject(&quot;Invoice__c&quot;);
		objInvoice.Id = &apos;{!Invoice__c.Id}&apos;;
		objInvoice.Status__c = &apos;Send Email&apos;;
		var result = sforce.connection.update([objInvoice]);
		if(result[0].getBoolean(&quot;success&quot;))
		{
		   alert(&apos;Invoice sent successfully&apos;);
		   window.location.reload();
		}
		else
		{
		  alert(&apos;Error : &apos;+result);
		}
	}		
}</url>
