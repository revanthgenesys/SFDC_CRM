    <fields>
        <fullName>Blog_Content__c</fullName>
        <description>The rich text content of the blog post.</description>
        <externalId>false</externalId>
        <label>Blog Content</label>
        <length>32768</length>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Html</type>
        <visibleLines>25</visibleLines>
    </fields>
    <fields>
        <fullName>Blog_Post_Url__c</fullName>
        <externalId>false</externalId>
        <formula>HYPERLINK(&apos;/partner/Partner_Portal_Blog_Post?id=&apos;+Id,  Name )</formula>
        <label>Blog Post Url</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Expiration_Date__c</fullName>
        <description>The date and  time at which the blog post will no longer be displayed and available.  If omitted, the blog post will remain until it is removed.</description>
        <externalId>false</externalId>
        <label>Blog Expiration Date</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>DateTime</type>
    </fields>
    <fields>
        <fullName>Home_Page_Flag__c</fullName>
        <defaultValue>false</defaultValue>
        <description>check to display this post on the partner portal home page blog spotlight section</description>
        <externalId>false</externalId>
        <label>Blog Used on Home Page</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Publication_Date__c</fullName>
        <description>The date and time at which this blog post should become visible.  If omitted, the blog post will become immediately visible.</description>
        <externalId>false</externalId>
        <label>Blog Publication Date</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>DateTime</type>
    </fields>
    <label>Blog Post</label>
    <listViews>
        <fullName>All</fullName>
        <columns>NAME</columns>
        <columns>Blog_Content__c</columns>
        <columns>CREATED_DATE</columns>
        <filterScope>Everything</filterScope>
        <label>All</label>
        <language>en_US</language>
        <sharedTo>
            <allInternalUsers></allInternalUsers>
        </sharedTo>
    </listViews>
    <listViews>
        <fullName>Partner_Portal_Blog_List</fullName>
        <booleanFilter>(1 OR 3) AND 2</booleanFilter>
        <columns>Blog_Post_Url__c</columns>
        <columns>Publication_Date__c</columns>
        <columns>Expiration_Date__c</columns>
        <filterScope>Everything</filterScope>
        <filters>
            <field>Expiration_Date__c</field>
            <operation>greaterThan</operation>
            <value>TODAY</value>
        </filters>
        <filters>
            <field>Publication_Date__c</field>
            <operation>lessThan</operation>
            <value>TODAY</value>
        </filters>
        <filters>
            <field>Expiration_Date__c</field>
            <operation>equals</operation>
        </filters>
        <label>Partner Portal Blog List</label>
        <language>en_US</language>
    </listViews>
