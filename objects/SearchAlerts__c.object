<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>Hierarchy</customSettingsType>
    <description>Parameters for SearchAlert visual force page, used by Coveo Search Alert to redirect users to proper community</description>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>Customer_Care_Host_Name__c</fullName>
        <defaultValue>&quot;https://sbgenfull2-genesyspartner.cs11.force.com/customercare&quot;</defaultValue>
        <description>Must be in the form: https://hostname/path</description>
        <externalId>false</externalId>
        <label>Customer Care Host Name</label>
        <length>255</length>
        <required>true</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Internal_Host_Name__c</fullName>
        <defaultValue>&quot;https://genesys--sbgenfull2.cs11.my.salesforce.com&quot;</defaultValue>
        <description>host name of the &quot;internal&quot; salesforce instance i.e. used by Support Agents.</description>
        <externalId>false</externalId>
        <inlineHelpText>host name of the &quot;internal&quot; salesforce instance i.e. used by Support Agents. Use format: https://hostname</inlineHelpText>
        <label>Internal Host Name</label>
        <length>255</length>
        <required>true</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <label>SearchAlerts</label>
    <visibility>Public</visibility>
</CustomObject>
