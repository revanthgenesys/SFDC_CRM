    <validationRules>
        <fullName>Delivery_Required</fullName>
        <active>false</active>
        <description>At least one messaged delivery platform required</description>
        <errorConditionFormula>NOT(OR( Premise__c  
 , Cloud__c    ))</errorConditionFormula>
        <errorDisplayField>Cloud__c</errorDisplayField>
        <errorMessage>At least one Delivery Platform is required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Demos_Presented_Required</fullName>
        <active>false</active>
        <errorConditionFormula>(ISPICKVAL(Type__c, &quot;Genesys Event&quot;) ||
ISPICKVAL(Type__c, &quot;Tradeshow&quot;)) &amp;&amp;
len( Demos_Presented__c )  &lt; 5</errorConditionFormula>
        <errorDisplayField>Demos_Presented__c</errorDisplayField>
        <errorMessage>Demos Presented is required for all Field Events</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Description_field_50Char_Min</fullName>
        <active>false</active>
        <description>This rule make it so the character length is at least 50 char long</description>
        <errorConditionFormula>len( Description__c )  &lt; 50</errorConditionFormula>
        <errorDisplayField>Description__c</errorDisplayField>
        <errorMessage>Please provide a detailed description longer than 50 characters</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>End_Date_Required_for_Ongoing</fullName>
        <active>false</active>
        <description>Requires an End Date to be populated when Ongoing is checked</description>
        <errorConditionFormula>and(Ongoing__c = true, isblank(End_Date__c))</errorConditionFormula>
        <errorMessage>Ongoing Activities must have an End Date. Please enter an End Date below</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Event_Date_Required</fullName>
        <active>false</active>
        <description>If program type = Webinar, Genesys Event or Tradeshow then Event Date field is required.</description>
        <errorConditionFormula>AND( 
OR( 
ISPICKVAL( Type__c , &quot;Webinar&quot;), 
ISPICKVAL( Type__c , &quot;Genesys Event&quot;), 
ISPICKVAL( Type__c , &quot;Tradeshow&quot;)), 
ISBLANK(Event_Date__c) 
)</errorConditionFormula>
        <errorDisplayField>Event_Date__c</errorDisplayField>
        <errorMessage>Please enter an Event Date. This field is mandatory for Webinars, Genesys Events and Tradeshows.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Follow_Up_Plan_Required</fullName>
        <active>false</active>
        <errorConditionFormula>(ISPICKVAL(Type__c, &quot;Genesys Event&quot;) ||
ISPICKVAL(Type__c, &quot;Tradeshow&quot;) ||
ISPICKVAL(Type__c, &quot;Webinar&quot;)) &amp;&amp;
len( Plan__c )  &lt; 25</errorConditionFormula>
        <errorDisplayField>Plan__c</errorDisplayField>
        <errorMessage>Please provide a detailed follow up plan longer than 25 characters</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Genesys_Involvement_when_Event_is_picked</fullName>
        <active>false</active>
        <description>This rule makes the Genesys Involvement field required when Program Type is either &quot;Genesys Event&quot; or &quot;Tradeshow&quot;.</description>
        <errorConditionFormula>and(isblank(Genesys_Involvement_in_Event__c), 
OR(ISPICKVAL(Type__c, &quot;Genesys Event&quot;),
ISPICKVAL(Type__c, &quot;Tradeshow&quot;)))</errorConditionFormula>
        <errorDisplayField>Genesys_Involvement_in_Event__c</errorDisplayField>
        <errorMessage>Please select Genesys&apos; Involvement in the Event</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LDR_Follow_Up_Rejection_Reason_Required</fullName>
        <active>false</active>
        <errorConditionFormula>AND 
( 
ISPICKVAL(Program_Follow_Up__c ,&quot;LDR Follow up Rejected&quot;), 
ISBLANK(Follow_Up_Rejection_Reason__c) 
)</errorConditionFormula>
        <errorDisplayField>Follow_Up_Rejection_Reason__c</errorDisplayField>
        <errorMessage>Please provide a reason for rejection.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LDR_Followup_Approved</fullName>
        <active>false</active>
        <description>Only Russell, Lead Development Reps &amp; Marketing Ops can change the `Program Follow Up` field’s value to &quot;LDR Follow up Approved&quot;. User IDs are hard coded for Russell and Marketing Operations.</description>
        <errorConditionFormula>AND( 
ISCHANGED(Program_Follow_Up__c), 
TEXT(Program_Follow_Up__c) = &quot;LDR Follow up Approved&quot;, 
$User.Id &lt;&gt; &quot;005U0000000dwSP&quot;, 
$User.Id &lt;&gt; &quot;005U0000000eLQV&quot;, 
$User.Id &lt;&gt; &quot;005U0000000dwUM&quot;, 
$User.Id &lt;&gt; &quot;005U0000000dwUF&quot;, 
$User.Id &lt;&gt; &quot;005U0000000dwSq&quot;,
$User.Id &lt;&gt; &quot;005U0000003Mghr&quot;,
$User.ProfileId&lt;&gt;&quot;00eU0000000dJjW&quot;,
$User.ProfileId&lt;&gt;&quot;00eU0000000dJiL&quot;)</errorConditionFormula>
        <errorMessage>You do not have permission to change this field.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MFM_ID_Reqiured</fullName>
        <active>false</active>
        <errorConditionFormula>OR( 
$User.ProfileId &lt;&gt; &quot;00eU0000000dJiL&quot;, 
$User.ProfileId &lt;&gt; &quot;00eU0000000dJjW&quot;, 
$User.ProfileId &lt;&gt; &quot;00eU0000000e70X&quot;, 
$User.ProfileId &lt;&gt; &quot;00eU0000000dfWW&quot;, 
$User.ProfileId &lt;&gt; &quot;00eU0000000dWZr&quot;, 
$User.ProfileId &lt;&gt; &quot;00eU0000000pPrr&quot;)</errorConditionFormula>
        <errorDisplayField>MFM_ID__c</errorDisplayField>
        <errorMessage>Please include the Marketing Program&apos;s MFM ID</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Minimum_50_Characters</fullName>
        <active>false</active>
        <errorConditionFormula>len(  Synopsis__c )  &lt; 50</errorConditionFormula>
        <errorDisplayField>Synopsis__c</errorDisplayField>
        <errorMessage>Please provide a detailed synopsis &amp; objective longer than 50 characters</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PartnerEvent_Start_and_End_Date_required</fullName>
        <active>false</active>
        <errorConditionFormula>IF( 
(Publish_to_Partner_Portal__c == true &amp;&amp; 
( ISBLANK(Partner_Event_Start_Date__c) ||  ISBLANK(Partner_Event_End_Date__c) )), 
TRUE, FALSE)</errorConditionFormula>
        <errorMessage>Partner Event Start Date and Partner Event End Date are required when Publish to Partner Portal is selected</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Platform_Required</fullName>
        <active>false</active>
        <description>At least one messaged platform required</description>
        <errorConditionFormula>NOT(OR( PureEngage__c 
 , PureConnect__c 
 , PureCloud__c  ))</errorConditionFormula>
        <errorDisplayField>PureCloud__c</errorDisplayField>
        <errorMessage>At least one Platform is required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Send_to_Nurture_not_checked</fullName>
        <active>false</active>
        <errorConditionFormula>Send_to_Nurture__c  = FALSE &amp;&amp;  ISBLANK(Send_to_Nurture_Reason__c)</errorConditionFormula>
        <errorDisplayField>Send_to_Nurture_Reason__c</errorDisplayField>
        <errorMessage>Please leave a note on why the leads are not going into nurture.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Speaker_Names_and_Titles_Required</fullName>
        <active>false</active>
        <errorConditionFormula>(ISPICKVAL(Type__c, &quot;Genesys Event&quot;) ||
ISPICKVAL(Type__c, &quot;Tradeshow&quot;) ||
ISPICKVAL(Type__c, &quot;Webinar&quot;))
 &amp;&amp;  LEN(Speakers__c) &lt; 5</errorConditionFormula>
        <errorDisplayField>Speakers__c</errorDisplayField>
        <errorMessage>Speaker Names and Titles are required for all Field Events and Webinars.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Sub_Region_Required</fullName>
        <active>false</active>
        <description>This makes Sub Region required unless the value Global is chosen for Region</description>
        <errorConditionFormula>(and(ispickval(Sub_Region__c, &quot;&quot;),not(ISPICKVAL(Region__c, &quot;GLOBAL&quot;))))</errorConditionFormula>
        <errorMessage>Please enter a Sub_Region</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Targeted_Segment_Required</fullName>
        <active>false</active>
        <description>At least one targeted segment required</description>
        <errorConditionFormula>NOT(OR(Enterprise__c
 ,Commercial__c 
 ,Mid_Market__c 
 ,Velocity__c ))</errorConditionFormula>
        <errorDisplayField>Velocity__c</errorDisplayField>
        <errorMessage>At least one Target Market Segment is required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Venue_Required</fullName>
        <active>false</active>
        <errorConditionFormula>(ISPICKVAL(Type__c, &quot;Genesys Event&quot;) ||
ISPICKVAL(Type__c, &quot;Tradeshow&quot;)) &amp;&amp;
len( Venue__c )  &lt; 5</errorConditionFormula>
        <errorDisplayField>Venue__c</errorDisplayField>
        <errorMessage>Venue is required for all Field Events</errorMessage>
    </validationRules>
