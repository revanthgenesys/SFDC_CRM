    <fields>
        <fullName>AQI_Score__c</fullName>
        <externalId>false</externalId>
        <formula>(IF( TEXT(Answer_1__c) = &apos;No&apos; ,3 , 0) +  
IF( TEXT(Answer_2__c) = &apos;Yes&apos; ,1 , 0) +
IF( TEXT(Answer_3__c) = &apos;Yes&apos; ,2 , 0) +
IF( TEXT(Answer_4__c) = &apos;Yes&apos; ,1 , 0) +
IF( TEXT(Answer_5__c) = &apos;Yes&apos; ,1 , 0) +
IF( TEXT(Answer_6__c) = &apos;Yes&apos; ,2 , 0) +
IF( TEXT(Answer_7__c) = &apos;Yes&apos; ,2 , 0) )/12</formula>
