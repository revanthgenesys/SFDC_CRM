        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Complete Audit</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&apos;/soap/ajax/20.0/connection.js&apos;)}
var auditObj = new sforce.SObject(&apos;AQI_Audit__c&apos;);
    auditObj.Id = &apos;{!AQI_Audit__c.Id}&apos;;
    auditObj.set(&apos;Status__c&apos;,&apos;Complete&apos;);
    var result = sforce.connection.update([auditObj]);

    if (result[0].success==&apos;false&apos;) {
         alert(result[0].errors.message);
    } else {
         location.reload(true);
    }</url>
    </webLinks>
