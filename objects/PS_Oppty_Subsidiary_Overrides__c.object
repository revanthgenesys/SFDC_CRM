<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>List</customSettingsType>
    <description>Subsidiary overrides for the PS Opportunities, to set the subsidiary number to be used for invoicing when the Opportunity sold to is in a given region and country</description>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>Country__c</fullName>
        <description>The country in which the PS Opportunity Solt To account is located</description>
        <externalId>false</externalId>
        <label>Country</label>
        <length>60</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Region__c</fullName>
        <description>The region in which the PS Opportunity has been sold</description>
        <externalId>false</externalId>
        <label>Region</label>
        <length>30</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Subsidiary_Override__c</fullName>
        <description>The Subsidiary Override value that should be used when the Region for the Opportunity and the Sold To Account Country are matched. This is used for invoice/billing overrides as directed by Finance</description>
        <externalId>false</externalId>
        <label>Subsidiary Override</label>
        <length>8</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <label>PS Opportunity Subsidiary Overrides</label>
    <visibility>Public</visibility>
</CustomObject>
