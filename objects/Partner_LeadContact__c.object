    <allowInChatterGroups>false</allowInChatterGroups>
    <compactLayoutAssignment>SYSTEM</compactLayoutAssignment>
    <deploymentStatus>Deployed</deploymentStatus>
    <description>One record for each Partner relationship with a Lead or Contact, where that relationship exists.  Primarily used to keep track of the Status (Captured, Engaged, Opportunity, Recycle, etc.).</description>
    <enableActivities>false</enableActivities>
    <enableBulkApi>true</enableBulkApi>
    <enableFeeds>false</enableFeeds>
    <enableHistory>true</enableHistory>
    <enableLicensing>false</enableLicensing>
    <enableReports>true</enableReports>
    <enableSearch>false</enableSearch>
    <enableSharing>true</enableSharing>
    <enableStreamingApi>true</enableStreamingApi>
    <externalSharingModel>ReadWrite</externalSharingModel>
    <fields>
        <fullName>Contact__c</fullName>
        <deleteConstraint>SetNull</deleteConstraint>
        <description>Id of associated Contact (If exists)</description>
        <externalId>false</externalId>
        <label>Contact</label>
        <referenceTo>Contact</referenceTo>
        <relationshipLabel>Partner LeadContacts</relationshipLabel>
        <relationshipName>Partner_LeadContacts</relationshipName>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>External_Id__c</fullName>
        <caseSensitive>true</caseSensitive>
        <externalId>true</externalId>
        <label>External Id</label>
        <length>255</length>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>true</unique>
    </fields>
    <fields>
        <fullName>Lead__c</fullName>
        <deleteConstraint>SetNull</deleteConstraint>
        <description>Id of associated Lead (If exists)</description>
        <externalId>false</externalId>
        <label>Lead</label>
        <referenceTo>Lead</referenceTo>
        <relationshipLabel>Partner LeadContacts</relationshipLabel>
        <relationshipName>Partner_LeadContacts</relationshipName>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>Partner_Account__c</fullName>
        <deleteConstraint>Restrict</deleteConstraint>
        <description>Id of Partner Account working with the Lead or Contact.</description>
        <externalId>false</externalId>
        <label>Partner Account</label>
        <lookupFilter>
            <active>true</active>
            <errorMessage>Value does not exist or is not a Partner account.</errorMessage>
            <filterItems>
                <field>Account.RecordTypeId</field>
                <operation>equals</operation>
                <value>Enterprise - Business Partner</value>
            </filterItems>
            <isOptional>false</isOptional>
        </lookupFilter>
        <referenceTo>Account</referenceTo>
        <relationshipLabel>Partner LeadContacts</relationshipLabel>
        <relationshipName>Partner_LeadContacts</relationshipName>
        <required>true</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>Recycle_Reason__c</fullName>
        <description>Reason that the Status was set to Recycle.</description>
        <externalId>false</externalId>
        <inlineHelpText>Reason that the Status was set to Recycle.</inlineHelpText>
        <label>Recycle Reason</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>TextArea</type>
    </fields>
    <fields>
        <fullName>Status__c</fullName>
        <description>Status of the Lead or Contact in relation to the Partner Account.</description>
        <externalId>false</externalId>
        <label>Status</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Picklist</type>
        <valueSet>
            <restricted>true</restricted>
            <valueSetDefinition>
                <sorted>false</sorted>
                <value>
                    <fullName>Captured</fullName>
                    <default>true</default>
                    <label>Captured</label>
                </value>
                <value>
                    <fullName>Engaged</fullName>
                    <default>false</default>
                    <label>Engaged</label>
                </value>
                <value>
                    <fullName>Recycled</fullName>
                    <default>false</default>
                    <label>Recycled</label>
                </value>
                <value>
                    <fullName>Deal/Opportunity Created</fullName>
                    <default>false</default>
                    <label>Deal/Opportunity Created</label>
                </value>
                <value>
                    <fullName>Deal Registration</fullName>
                    <default>false</default>
                    <isActive>false</isActive>
                    <label>Deal Registration</label>
                </value>
                <value>
                    <fullName>Opportunity</fullName>
                    <default>false</default>
                    <isActive>false</isActive>
                    <label>Opportunity</label>
                </value>
                <value>
                    <fullName>Recycle</fullName>
                    <default>false</default>
                    <isActive>false</isActive>
                    <label>Recycle</label>
                </value>
            </valueSetDefinition>
        </valueSet>
    </fields>
    <label>Partner LeadContact</label>
    <nameField>
        <displayFormat>PLC-{000000}</displayFormat>
        <label>Partner LeadContact Name</label>
        <trackHistory>false</trackHistory>
        <type>AutoNumber</type>
    </nameField>
    <pluralLabel>Partner LeadContacts</pluralLabel>
    <searchLayouts/>
    <sharingModel>ReadWrite</sharingModel>
    <validationRules>
        <fullName>Partner_LeadContact_Recycle_Reason</fullName>
        <active>true</active>
        <description>Requires Recycle Reason when Status is set to Recycle.</description>
        <errorConditionFormula>AND(
 ISPICKVAL( Status__c , &apos;Recycle&apos;) ,
 ISBLANK( Recycle_Reason__c ) 
)</errorConditionFormula>
        <errorDisplayField>Recycle_Reason__c</errorDisplayField>
        <errorMessage>Please include a recycle reason when setting the status to recycle.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Partner_LeadContact_Required_LeadContact</fullName>
        <active>true</active>
        <description>Requires either Lead or Contact to be populated.</description>
        <errorConditionFormula>AND(
 ISBLANK( Lead__c ) ,
 ISBLANK( Contact__c ) 
)</errorConditionFormula>
        <errorMessage>Please include a contact or lead.</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>
