    <webLinks>
        <fullName>Generate_CaaS_Proposal</fullName>
        <availability>online</availability>
        <description>window.location.href = URL;
window.open( URL, &quot;Conga&quot;, &quot;width=300,height=150,menubar=0&quot; );</description>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Generate CaaS Proposal</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/20.0/connection.js&quot;)}
var soql= &quot;SELECT id FROM APXTConga4__Conga_Template__c WHERE Tier__c INCLUDES (&apos;&quot; + &quot;{!Quote__c.Tier__c}&quot; + &quot;&apos;) AND Language__c = &apos;&quot; + &quot;{!Quote__c.Language__c}&quot; + &quot;&apos;&quot;;

var templates = sforce.connection.query(soql);

if (templates.size == 0) {
    soql = &quot;SELECT id FROM APXTConga4__Conga_Template__c WHERE Tier__c INCLUDES (&apos;&quot; + &quot;{!Quote__c.Tier__c}&quot; + &quot;&apos;) AND Language__c = &apos;English - US&apos;&quot;;
    templates = sforce.connection.query(soql);
}

if (templates.size &gt; 0) {
    var template = templates.getArray(&quot;records&quot;)[0];
    var URL = &quot;https://www.appextremes.com/apps/Conga/Composer.aspx&quot; +
    &quot;?SessionId={!API.Session_ID}&quot; +
    &quot;&amp;ServerUrl={!API.Partner_Server_URL_210}&quot; +
    &quot;&amp;Id={!Quote__c.Id}&quot; +
    &quot;&amp;DS7=1&quot; +
    &quot;&amp;TemplateID=&quot; + template.Id +
    &quot;&amp;ReportID=[Monthly]00OE0000001N991?pv0={!Quote__c.Id}&quot; +
    &quot;&amp;ReportID=[Hardware]00OE0000001N98y?pv0={!Quote__c.Id}&quot; +
    &quot;&amp;ReportID=[Telco]00OE0000001N992?pv0={!Quote__c.Id}&quot; +
    &quot;&amp;ReportID=[ProfImpl]00OE0000001N98z?pv0={!Quote__c.Id}&quot; +
    &quot;&amp;ReportID=[MPLS]00OE0000001N990?pv0={!Quote__c.Id}&quot; +
    &quot;&amp;ReportID=[DCProv]00OE0000001N98x?pv0={!Quote__c.Id}&quot; +
    &quot;&amp;ReportID=[Education]00OE0000001NVwY?pv0={!Quote__c.Id}&quot; +
    &quot;&amp;OFN={!Quote__c.Account__c}+-+Proposal+-+{!TODAY()}+-+v000&quot;;
    // alert(URL);
    window.location.href = URL;
} else {
    alert(&quot;There is no template defined for {!Quote__c.Tier__c} - {!Quote__c.Language__c}.  There is also no default template assigned.&quot;);
}</url>
    </webLinks>
