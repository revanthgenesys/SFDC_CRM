        <externalId>false</externalId>
        <formula>/*Commented on request of Vivek on 21/5/2019.
ABS(Case( Quarter_Upside_Risk__c,
&apos;Risk - Q1 - 2018&apos;, Account_Revenue_Forecast__r.Q1_2018_Low__c,
&apos;Risk - Q2 - 2018&apos;, Account_Revenue_Forecast__r.Q2_2018_Low__c,
&apos;Risk - Q3 - 2018&apos;, Account_Revenue_Forecast__r.Q3_2018_Low__c,
&apos;Risk - Q4 - 2018&apos;, Account_Revenue_Forecast__r.Q4_2018_Low__c,
&apos;Risk - Q1 - 2019&apos;, Account_Revenue_Forecast__r.Q1_2019_Low__c,
&apos;Risk - Q2 - 2019&apos;, Account_Revenue_Forecast__r.Q2_2019_Low__c,
&apos;Risk - Q3 - 2019&apos;, Account_Revenue_Forecast__r.Q3_2019_Low__c,
&apos;Risk - Q4 - 2019&apos;, Account_Revenue_Forecast__r.Q4_2019_Low__c,
&apos;Upside - Q1 - 2019&apos;, Account_Revenue_Forecast__r.Q1_2019_Best__c,
&apos;Upside - Q1 - 2018&apos;, Account_Revenue_Forecast__r.Q1_2018_Best__c,
&apos;Upside - Q2 - 2018&apos;, Account_Revenue_Forecast__r.Q2_2018_Best__c,
&apos;Upside - Q3 - 2018&apos;, Account_Revenue_Forecast__r.Q3_2018_Best__c,
&apos;Upside - Q4 - 2018&apos;, Account_Revenue_Forecast__r.Q4_2018_Best__c,
&apos;Upside - Q2 - 2019&apos;, Account_Revenue_Forecast__r.Q2_2019_Best__c,
&apos;Upside- Q3 - 2019&apos;, Account_Revenue_Forecast__r.Q3_2019_Best__c,
&apos;Upside- Q4 - 2019&apos;, Account_Revenue_Forecast__r.Q4_2019_Best__c,
&apos;Upside- Q1 - 2019&apos;, Account_Revenue_Forecast__r.Q1_2019_Best__c,
&apos;Risk - ROY - 2018&apos;, Account_Revenue_Forecast__r.Q2_2018_Low__c + Account_Revenue_Forecast__r.Q3_2018_Low__c + Account_Revenue_Forecast__r.Q4_2018_Low__c,
&apos;Risk - ROY - 2019&apos;, Account_Revenue_Forecast__r.Q2_2019_Low__c + Account_Revenue_Forecast__r.Q3_2019_Low__c + Account_Revenue_Forecast__r.Q4_2019_Low__c,
&apos;Upside - ROY - 2018&apos;, Account_Revenue_Forecast__r.Q2_2018_Best__c + Account_Revenue_Forecast__r.Q3_2018_Best__c + Account_Revenue_Forecast__r.Q4_2018_Best__c,
&apos;Upside - ROY - 2019&apos;, Account_Revenue_Forecast__r.Q2_2019_Best__c + Account_Revenue_Forecast__r.Q3_2019_Best__c + Account_Revenue_Forecast__r.Q4_2019_Best__c,


0))
*/

ABS(Case( Quarter_Upside_Risk__c, 
&apos;Risk - Q1 - 2019&apos;, Account_Revenue_Forecast__r.Q1_2019_Low__c,
&apos;Risk - Q2 - 2019&apos;, Account_Revenue_Forecast__r.Q2_2019_Low__c, 
&apos;Risk - Q3 - 2019&apos;, Account_Revenue_Forecast__r.Q3_2019_Low__c, 
&apos;Risk - Q4 - 2019&apos;, Account_Revenue_Forecast__r.Q4_2019_Low__c,
&apos;Risk - Q1 - 2020&apos;, Account_Revenue_Forecast__r.Q1_2020_Low__c, 
&apos;Risk - Q2 - 2020&apos;, Account_Revenue_Forecast__r.Q2_2020_Low__c, 
&apos;Risk - Q3 - 2020&apos;, Account_Revenue_Forecast__r.Q3_2020_Low__c, 
&apos;Risk - Q4 - 2020&apos;, Account_Revenue_Forecast__r.Q4_2020_Low__c,
&apos;Upside - Q1 - 2019&apos;, Account_Revenue_Forecast__r.Q1_2019_Best__c, 
&apos;Upside - Q2 - 2019&apos;, Account_Revenue_Forecast__r.Q2_2019_Best__c, 
&apos;Upside - Q3 - 2019&apos;, Account_Revenue_Forecast__r.Q3_2019_Best__c, 
&apos;Upside - Q4 - 2019&apos;, Account_Revenue_Forecast__r.Q4_2019_Best__c,
&apos;Upside - Q1 - 2020&apos;, Account_Revenue_Forecast__r.Q1_2020_Best__c, 
&apos;Upside - Q2 - 2020&apos;, Account_Revenue_Forecast__r.Q2_2020_Best__c, 
&apos;Upside - Q3 - 2020&apos;, Account_Revenue_Forecast__r.Q3_2020_Best__c, 
&apos;Upside - Q4 - 2020&apos;, Account_Revenue_Forecast__r.Q4_2020_Best__c,
&apos;Risk - ROY - 2019&apos;, Account_Revenue_Forecast__r.Q2_2019_Low__c + Account_Revenue_Forecast__r.Q3_2019_Low__c + Account_Revenue_Forecast__r.Q4_2019_Low__c, 
&apos;Risk - ROY - 2020&apos;, Account_Revenue_Forecast__r.Q2_2020_Low__c + Account_Revenue_Forecast__r.Q3_2020_Low__c + Account_Revenue_Forecast__r.Q4_2020_Low__c, 
&apos;Upside - ROY - 2019&apos;, Account_Revenue_Forecast__r.Q2_2019_Best__c + Account_Revenue_Forecast__r.Q3_2019_Best__c + Account_Revenue_Forecast__r.Q4_2019_Best__c, 
