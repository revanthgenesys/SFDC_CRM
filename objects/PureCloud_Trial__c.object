    <validationRules>
        <fullName>Mandatory_Criteria</fullName>
        <active>true</active>
        <description>Mandatory Criteria prior to accept “Queued for Automation” value on PureCloud Project Auto Activation Status</description>
        <errorConditionFormula>AND(
  ISPICKVAL(Auto_Activation_Status__c,&quot;Queued for Automation&quot;),
  OR(
    ISBLANK(TEXT( AWS_Predefined_Region__c )),
    ISBLANK( Account__c),
    ISBLANK( Trial_Contact__c ),
    ISBLANK( Trial_Contact__r.Email),
    ISBLANK(TEXT(Contact_Language__c)),
    ISBLANK(Opportunity__c ),
    ISBLANK(Opportunity__r.Id ),
    ISBLANK( BMQuote__c ),
    ISBLANK( BMQuote__r.Id ),
    BMQuote__r.BigMachines__Status__c != &apos;Order Accepted&apos;
  )
)</errorConditionFormula>
        <errorMessage>Project must have AWS Region, Account, Contact Email &amp; Language, Opportunity, Quote and Quote status (Order Accepted) before moving to Queued for Automation</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>UC_users_and_feature</fullName>
        <active>true</active>
        <description>The UC functionality option must be selected and a number of UC users entered, or the option must not be selected and no users must be entered.</description>
        <errorConditionFormula>OR(
AND(
INCLUDES(Desired_General_Functionality__c , &quot;UC&quot;),
ISBLANK(Number_of_UC_Users__c)
),
AND(
NOT(ISBLANK(Number_of_UC_Users__c)),
NOT(INCLUDES(Desired_General_Functionality__c , &quot;UC&quot;))
)
)</errorConditionFormula>
        <errorMessage>For UC PureCloud requests, please select the UC general functionality option and enter a number of UC users.</errorMessage>
    </validationRules>
