    <validationRules>
        <fullName>Application_Check</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL( Request_Type__c , &apos;User Access&apos;) &amp;&amp; ISPICKVAL(  Application__c , &apos;&apos;)</errorConditionFormula>
        <errorDisplayField>Application__c</errorDisplayField>
        <errorMessage>Please provide Application when Request Type is User Access.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Application_Check_for_Training</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL( Request_Type__c , &apos;Training&apos;) &amp;&amp; ISPICKVAL( Application__c , &apos;&apos;)</errorConditionFormula>
        <errorDisplayField>Application__c</errorDisplayField>
        <errorMessage>Please provide Application when Request Type is Training.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Assigned_To_Check</fullName>
        <active>true</active>
        <description>If status is not equal to &apos;new&apos; or &apos;submitted&apos; must have a resource in &apos;&apos;assigned to&quot;</description>
        <errorConditionFormula>(ISPICKVAL(Support_Area__c,&apos;Sales Ops&apos;) ||
ISPICKVAL(Support_Area__c,&apos;Tools/Systems Support&apos;)||
ISPICKVAL(Support_Area__c,&apos;Quote Request&apos; )||
ISPICKVAL(Support_Area__c,&apos;Subscription PureSuccess&apos;)) &amp;&amp;  

(NOT(ISPICKVAL(Status__c ,&apos;New&apos;) || (ISPICKVAL(Status__c, &apos;Submitted&apos;))) &amp;&amp;

ISBLANK(Assigned_To__c) ||

ISNULL(Assigned_To__c))</errorConditionFormula>
        <errorDisplayField>Assigned_To__c</errorDisplayField>
        <errorMessage>Please assign appropriate resource to this DSR.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>DSR_Closure</fullName>
        <active>true</active>
        <description>Requires notes and resolution to be filled when moving DSR record to &apos;completed&apos;</description>
        <errorConditionFormula>(ISPICKVAL(Support_Area__c,&apos;Sales Ops&apos;) ||
ISPICKVAL(Support_Area__c,&apos;Tools/Systems Support&apos;)||
ISPICKVAL(Support_Area__c,&apos;Quote Request&apos;) ||
ISPICKVAL(Request_Type__c,&apos;Order Management On Premise&apos;)
)

&amp;&amp;

(ISPICKVAL(Status__c,&quot;Completed&quot;) &amp;&amp; 

(ISBLANK(Notes__c) || ISPICKVAL(Resolution__c,&quot;&quot;)))

&amp;&amp;  NOT($User.BypassValidationRules__c)</errorConditionFormula>
        <errorMessage>Please provide notes and a resolution type prior to moving the status to &apos;completed&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Date_Change_for_account_inactivation</fullName>
        <active>true</active>
        <description>Official Cancellation Date &amp; Notification Date Lock</description>
        <errorConditionFormula>AND(NOT($User.BypassValidationRules__c),
AND($Profile.Name &lt;&gt; &quot;GEN – Sales CSM&quot;,$Profile.Name &lt;&gt; &quot;System Administrator&quot;,$Profile.Name &lt;&gt; &quot;Data Administrator&quot;, $Profile.Name &lt;&gt; &quot;GEN - Finance&quot;), OR(
  ISCHANGED(Official_Cancelation_Date__c), ISCHANGED(Notification_Date__c),ISCHANGED(Product_Cancellation_Date__c)),Text(Owner_Approval__c )= &apos;Final Approval&apos;)</errorConditionFormula>
        <errorMessage>&apos;Owner Status = &quot;Final Approval&quot;. &apos;Official Cancellation Date&apos; &amp; &apos;Notification Date&apos; cannot be changed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Due_Date_Check</fullName>
        <active>true</active>
        <errorConditionFormula>AND( Request_Due_Date__c &lt; TODAY(),
OR(ISNEW(),ISCHANGED(Request_Due_Date__c)) )</errorConditionFormula>
        <errorDisplayField>Request_Due_Date__c</errorDisplayField>
        <errorMessage>Request Due Date cannot be set prior to today.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Error_Message_Check</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL( Request_Type__c , &apos;Opportunity/Quote Error&apos;) &amp;&amp;  ISBLANK(Error_message__c)</errorConditionFormula>
        <errorDisplayField>Error_message__c</errorDisplayField>
        <errorMessage>Please enter Error Message when Request type is Opportunity/Quote Error.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Focus_Area_Check</fullName>
        <active>true</active>
        <description>If the Request Type= data Management the it is manadatory</description>
        <errorConditionFormula>ISPICKVAL(Support_Area__c,&apos;Sales Ops&apos;)&amp;&amp; ISPICKVAL( Request_Type__c , &apos;Data Management&apos;) &amp;&amp;  ISPICKVAL( Focus_Area__c , &apos;&apos;)</errorConditionFormula>
        <errorDisplayField>Focus_Area__c</errorDisplayField>
        <errorMessage>Please provide a Data Type when Request type is Data Management.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PS_Limit_BE_Edit_by_Request_Type</fullName>
        <active>true</active>
        <description>For PS: Limit the ability to edit the BE dates from the acceptance section unless the Request Type is BE.</description>
        <errorConditionFormula>AND( 
NOT ISNEW(), 
(RecordType.Name = &quot;Professional Services - DS&quot;), 
NOT (ISPICKVAL( Request_Type__c ,&quot;Budgetary Estimate (BE)&quot;) ), 
OR(ISCHANGED( BE_Request_Date__c ), 
ISCHANGED( BE_PS_Acceptance_Date__c ), 
ISCHANGED( BE_Delivery_Date__c ), 
ISCHANGED( BE_Sales_Acceptance_Date__c )), 
NOT ($User.BypassValidationRules__c ) 
)</errorConditionFormula>
        <errorMessage>BE Dates cannot be edited as this PS DSR Request Type is not related to Budgetary Estimates</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PS_Limit_SOW_Edit_by_Request_Type</fullName>
        <active>true</active>
        <description>For PS: Limit the ability to edit the SOW dates from the acceptance section unless the Request Type is SOW.</description>
        <errorConditionFormula>AND(
NOT ISNEW(),
(RecordType.Name = &quot;Professional Services - DS&quot;),
NOT (ISPICKVAL( Request_Type__c ,&quot;Statement of Work/Letter of Engagement (SOW/LOE)&quot;) ),
OR(ISCHANGED( SOW_Request_Date__c ),
ISCHANGED( SOW_PS_Acceptance_Date__c ),
ISCHANGED( SOW_Delivery_Date__c ),
ISCHANGED( SOW_Sales_Acceptance_Date__c )),
NOT ($User.BypassValidationRules__c )
)</errorConditionFormula>
        <errorMessage>SOW Dates cannot be edited as this PS DSR Request Type is not related to SOWs</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Play_Support_error</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT ($User.BypassValidationRules__c ), ISPICKVAL(Support_Area__c, &apos;Play Support&apos;), 
ISPICKVAL(Request_Type__c, &apos;Play Assistance&apos;), 
ISBLANK(TEXT(Primary_Play__c)))</errorConditionFormula>
        <errorMessage>Primary Play is required for Play support</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PureSuccess_offer_requirement</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL(Request_Type__c, &apos;PureSuccess Center&apos;) 
&amp;&amp; ISBLANK(Text(Prescribed_PureSuccess_Offer__c))  
&amp;&amp; ISPICKVAL(Status__c, &apos;Completed&apos;)</errorConditionFormula>
        <errorDisplayField>Prescribed_PureSuccess_Offer__c</errorDisplayField>
        <errorMessage>For Request Type: PureSuccess Center, please choose a Prescribed PureSuccess Offer prior to completing the request</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Quote_check</fullName>
        <active>true</active>
        <description>if Focus Area is quote the it is mandatory to insert Quote</description>
        <errorConditionFormula>ISPICKVAL(Focus_Area__c , &apos;Quote&apos;) &amp;&amp;  ISBLANK(  Quote__c ) &amp;&amp; NOT(ISPICKVAL(Request_Type__c, &apos;General Inquiry&apos;))</errorConditionFormula>
        <errorDisplayField>Quote__c</errorDisplayField>
        <errorMessage>Please enter Quote ID when Data Type is Quote.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Record_lock_final_approval</fullName>
        <active>true</active>
        <description>Official Cancellation Date &amp; Notification Date Lock</description>
        <errorConditionFormula>AND(NOT($User.BypassValidationRules__c),
AND($Profile.Name &lt;&gt; &quot;GEN – Sales CSM&quot;,$Profile.Name &lt;&gt; &quot;System Administrator&quot;,$Profile.Name &lt;&gt; &quot;Data Administrator&quot;, $Profile.Name &lt;&gt; &quot;GEN - Finance&quot;), OR(
ISCHANGED(CCDB_Cancelled__c), ISCHANGED(ProductDeactivated__c), ISCHANGED(Careinactivated__c)), TEXT(Owner_Approval__c) &lt;&gt; &apos;Final Approval&apos;)</errorConditionFormula>
        <errorMessage>&apos;Owner Status&apos; must = &quot;Final Approved&quot; before &quot;CCDB Cancelled&quot;, &quot;Product Deactivated&quot; or &quot;Care Inactivated&quot; can be set.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Request_Type_Check</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL( Request_Type__c , &apos;&apos;)</errorConditionFormula>
        <errorDisplayField>Request_Type__c</errorDisplayField>
        <errorMessage>Please select a request type.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Sitevisit_Geographic_Radi_field_required</fullName>
        <active>true</active>
        <errorConditionFormula>AND(

NOT($User.BypassValidationRules__c = True),

TEXT( Type_of_Reference_Activity__c) = &quot;Site Visit&quot;,
ISBLANK(TEXT( Site_Visit_Geographic_Radius__c)))</errorConditionFormula>
        <errorDisplayField>Site_Visit_Geographic_Radius__c</errorDisplayField>
        <errorMessage>This field should be mandatory, if selecting Site Visit in Reference Activity</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Status_Check</fullName>
        <active>false</active>
        <description>If the previous status is New then status should be &apos;in-progress&apos; or &apos;completed&apos;</description>
        <errorConditionFormula>(ISPICKVAL(Support_Area__c,&apos;Sales Ops&apos;) ||
ISPICKVAL(Support_Area__c,&apos;Tools/Systems Support&apos;)||
ISPICKVAL(Support_Area__c,&apos;Quote Request&apos; )) &amp;&amp; 

(ISPICKVAL(PRIORVALUE(Status__c),&apos;New&apos;) 
||ISPICKVAL(Status__c , &apos;New&apos;)) 
&amp;&amp;
(NOT(
ISPICKVAL(Status__c , &apos;In Progress&apos;) 
|| ISPICKVAL(Status__c , &apos;Assigned&apos;)
))
&amp;&amp; 
NOT(ISNEW()) &amp;&amp; NOT( ISCHANGED( Support_Area__c )) &amp;&amp; (IsNew__c) &amp;&amp; NOT(ISCHANGED(IsNew__c))</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>Please select status as In Progress.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Subject_Check</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK( Subject__c ) || ISNULL(Description__c) || IF((Description__c) ==Null,true,false)</errorConditionFormula>
        <errorDisplayField>Subject__c</errorDisplayField>
        <errorMessage>Please provide a Subject and Description of the support required.</errorMessage>
    </validationRules>
