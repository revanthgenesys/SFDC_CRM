        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/27.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/27.0/apex.js&quot;)} 

var instrQuery = &quot;SELECT Id FROM Account_Licensing_Instructions__c WHERE 	Account__c = &apos;{!Account.Id}&apos;&quot;;

result = sforce.connection.query(instrQuery);
records = result.getArray(&quot;records&quot;);
if ( records[0] ) {
 alert(&quot;Licensing Instructions have been already specified for this Account. If you need to make changes, please update or delete the existing record.&quot;);
} else {
var url=&apos;/aEs/e?retURL={!Account.Id}&apos;; 
url += &apos;&amp;CF00N0B0000065EDp={!Account.Name}&apos;;
