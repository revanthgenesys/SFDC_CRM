        <protected>false</protected>
        <requireRowSelection>false</requireRowSelection>
        <url>{!REQUIRESCRIPT(&apos;/soap/ajax/29.0/connection.js&apos;)}

var insRec = [];
var err = false;

var ji = new sforce.SObject(&quot;Jira_Issue_Link__c&quot;);
ji.Case__c =&quot;{!Case.Id}&quot;;
ji.JIL_Summary__c = &quot;{!JSENCODE(Case.Subject)}&quot;;
ji.Case_Description__c = &quot;{!JSENCODE(Case.Description)}&quot;;

var casetype = &quot;{!Case.RecordType}&quot;;
var url = &quot;&quot;;
if ( casetype.includes(&apos;Cloud&apos;) ) {
var pltf = &apos;{!Case.Cloud_Platform__c}&apos;;
if (pltf.includes(&quot;PureConnect&quot;))
ji.Case_Platform__c = &quot;PureConnect&quot;;
else if (pltf == &quot;PureCloud&quot;)
ji.Case_Platform__c = &quot;PureCloud&quot;;
else
ji.Case_Platform__c = &quot;PureEngage&quot;;
}
else
ji.Case_Platform__c = &apos;{!Case.BOA_Platform__c}&apos;;

insRec.push(ji);

var result = sforce.connection.create(insRec);
if ( !result[0].getBoolean( &quot;success&quot; ) ) {
   err = true;
   alert ( result[0].errors.message );
}

if ( err == false ) {
   var recid = result[0].id;

   if ( ji.Case_Platform__c.includes(&quot;PureEngage&quot;) )
       url = &quot;{!$Setup.JiraIntegration__c.PureEngageURL__c}&quot;;
    else if ( ji.Case_Platform__c.includes(&quot;PureConnect&quot;) )
       url = &quot;{!$Setup.JiraIntegration__c.PureConnectURL__c}&quot;;

    if ( url == &quot;&quot; )
      alert (&quot;Case Type is blank. Unable to determine JIRA instance&quot;);  
    else {
  
   url += &quot;/plugins/servlet/customware/connector/issue/1/Jira_Issue_Link__c/associate.action?&quot;;
   url += &quot;id=&quot; + recid;

   window.open(encodeURI(url), target=&quot;_blank&quot;);

   }
}</url>
