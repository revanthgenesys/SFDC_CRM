    <webLinks>
        <fullName>Create_Issue_in_PE_Jira</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Create Issue in PE Jira</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&apos;/soap/ajax/29.0/connection.js&apos;)}

var linked = false;
var query = &quot;SELECT JIL_Issue__c FROM Jira_Issue_Link__c WHERE id=&apos;&quot; + &quot;{!Jira_Issue_Link__c.Id}&quot; + &quot;&apos;&quot;;
var result = sforce.connection.query(query);
var records = result.getArray(&quot;records&quot;);
if(records[0] &amp;&amp; records[0].JIL_Issue__c != null &amp;&amp; records[0].JIL_Issue__c != &quot;&quot;) {
linked = true;
}

var casetype = &quot;{!Jira_Issue_Link__c.Jira_Instance__c}&quot;;

if ( casetype.includes(&quot;PureEngage&quot;) ) {
var url = &quot;{!$Setup.JiraIntegration__c.PureEngageURL__c}&quot;;

if ( linked == true ) {
alert (&quot;This record is already associated with JIRA Issue# &quot; + records[0].JIL_Issue__c + &quot;. Please refresh your browser to see the updated record details&quot;);
}
else if ( url == &quot;&quot; ) {
alert (&quot;Unable to determine JIRA instance for the case Platform &quot; + casetype);
}
else {
url += &quot;/plugins/servlet/customware/connector/issue/1/Jira_Issue_Link__c/create.action?&quot;;
url += &quot;id={!Jira_Issue_Link__c.Id}&quot;;
url += &quot;&amp;project={!Jira_Issue_Link__c.Target_Jira_Project__c}&quot;;
url += &quot;&amp;issueType=1&quot;;
url += &quot;&amp;summary=one&quot;;

window.open(url, target=&quot;_blank&quot;);
} }
else if ( casetype.includes(&quot;PureConnect&quot;) )
alert (&quot;Note: “Jira Instance” field is set to “PureConnect”. Integration with PureConnect instance of Jira is not supported&quot;);
else if ( casetype.includes(&quot;PureCloud&quot;) )
alert (&quot;Note: “Jira Instance” field is set to “PureCloud”. If you wish to create an Issue in PureCloud instance of Jira, use “Create or Link Issue in PureCloud Jira” section. Otherwise update “Jira Instance” field first&quot;);</url>
    </webLinks>
    <webLinks>
        <fullName>Create_JIRA_Issue</fullName>
        <availability>online</availability>
        <description>replaced by PE JIRA: Create New Issue</description>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Create New Jira Issue</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>false</requireRowSelection>
        <url>{!REQUIRESCRIPT(&apos;/soap/ajax/29.0/connection.js&apos;)}
{!REQUIRESCRIPT(&quot;/support/console/28.0/integration.js&quot;)}

var insRec = [];

var ji = new sforce.SObject(&quot;Jira_Issue_Link__c&quot;);
ji.Case__c = &apos;{!Case.Id}&apos;;
ji.JIL_Summary__c = &apos;{!JSENCODE(Case.Subject)}&apos;;
ji.Case_Description__c = &apos;{!JSENCODE(Case.Description)}&apos;;
ji.JIL_Priority__c = &apos;{!Case.Priority}&apos;;

var casetype = &apos;{!Case.RecordType}&apos;;
if ( casetype.includes(&apos;Cloud&apos;) ) {
	var pltf = &apos;{!Case.Cloud_Platform__c}&apos;;
	if (pltf.includes(&quot;PureConnect&quot;)) 
	   ji.Case_Platform__c = &quot;PureConnect&quot;;
	else if (pltf == &quot;PureCloud&quot;)
           ji.Case_Platform__c = &quot;PureCloud&quot;;
        else
	   ji.Case_Platform__c = &quot;PureEngage&quot;;
}
else
ji.Case_Platform__c = &apos;{!Case.BOA_Platform__c}&apos;;

var query = &quot;SELECT Component_Name__c, Version_Portal__c FROM Case WHERE id=&apos;&quot; + ji.Case__c + &quot;&apos;&quot;;
var result = sforce.connection.query(query);
var records = result.getArray(&quot;records&quot;);
if(records[0]) {
ji.JIL_Component__c = records[0].Component_Name__c;
ji.Jira_Affects_Versions__c = records[0].Version_Portal__c;
ji.Jira_Found_In_Version__c = records[0].Version_Portal__c;
} 

insRec.push(ji);
result = sforce.connection.create(insRec);
if ( !result[0].getBoolean( &quot;success&quot; ) ) {
alert ( result[0].errors.message );
}
else {
var recid = result[0].id;
var url = &quot;/&quot; + recid;
if (sforce.console.isInConsole()) {
   srcUp(url);
}
else {
   window.parent.location.href=url;
}
}</url>
    </webLinks>
    <webLinks>
        <fullName>Link_to_JIRA_Issue</fullName>
        <availability>online</availability>
