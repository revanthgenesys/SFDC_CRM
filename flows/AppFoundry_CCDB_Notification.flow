    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>NOT(ISBLANK([Subscription__c].Quote__c))
&amp;&amp;
(
NOT(ISPICKVAL([Subscription__c].Status__c,&quot;&quot;)) &amp;&amp;
ISCHANGED([Subscription__c].Status__c) &amp;&amp;
(ISPICKVAL([Subscription__c].Status__c,&quot;Cancelled&quot;) || ISPICKVAL([Subscription__c].Status__c,&quot;Inactive&quot;)))
&amp;&amp;
[Subscription__c].Platform_Type__c = &apos;Cloud&apos; 
&amp;&amp;
[Subscription__c].Quote__c.PrimQuoteHasAppFoundry__c = true
</stringValue>
            </value>
        </processMetadataValues>
