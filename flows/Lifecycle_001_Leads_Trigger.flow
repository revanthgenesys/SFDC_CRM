    <description>Initiates Lifecycle changes on score change or hot lead date update</description>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>OR(
ISNEW(),
BLANKVALUE([Lead].Behavior_Score_SFDC__c, 0) - BLANKVALUE(PRIORVALUE([Lead].Behavior_Score_SFDC__c), 0) &gt; 0,
BLANKVALUE([Lead].New_Behavior_Score__c , 0) - BLANKVALUE(PRIORVALUE([Lead].New_Behavior_Score__c ), 0) &gt; 0,
AND(ISCHANGED([Lead].Hot_Lead__c ) , [Lead].Recent_Hot_Lead__c )
)</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_1</name>
        <dataType>Boolean</dataType>
        <expression>OR(
ISNEW(),
BLANKVALUE({!myVariable_current.Behavior_Score_SFDC__c}, 0) - BLANKVALUE(PRIORVALUE({!myVariable_current.Behavior_Score_SFDC__c}), 0) &gt; 0,
BLANKVALUE({!myVariable_current.New_Behavior_Score__c} , 0) - BLANKVALUE(PRIORVALUE({!myVariable_current.New_Behavior_Score__c} ), 0) &gt; 0,
AND(ISCHANGED({!myVariable_current.Hot_Lead__c} ) , {!myVariable_current.Recent_Hot_Lead__c} )
)</expression>
    </formulas>
