            <name>originalFormula</name>
            <value>
                <stringValue>AND( ISPICKVAL([Opportunity].ForecastStatus__c , &apos;0.Booked&apos;) , NOT(ISCHANGED([Opportunity].Ownerid)), ISCHANGED([Opportunity].ForecastStatus__c) , 
OR([Opportunity].Country_Region__c = &apos;APAC&apos;,[Opportunity].Country_Region__c = &apos;North America&apos;,[Opportunity].Country_Region__c = &apos;LATAM&apos;))</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_7</name>
        <dataType>Boolean</dataType>
