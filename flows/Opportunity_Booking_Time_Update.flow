            <value>
                <stringValue>AND( ISPICKVAL([Opportunity].ForecastStatus__c , &apos;0.Booked&apos;) , ISCHANGED([Opportunity].ForecastStatus__c) ,  NOT(ISCHANGED([Opportunity].Ownerid)), [Opportunity].Country_Region__c = &apos;EMEA&apos;)</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_4</name>
        <dataType>Boolean</dataType>
        <expression>AND( ISPICKVAL({!myVariable_current.ForecastStatus__c} , &apos;0.Booked&apos;) , ISCHANGED({!myVariable_current.ForecastStatus__c}) ,  NOT(ISCHANGED({!myVariable_current.OwnerId})), {!myVariable_current.Country_Region__c} = &apos;EMEA&apos;)</expression>
    </formulas>
    <formulas>
