<apex:page controller="ProposalCloudController" applyBodyTag="false" standardStylesheets="false" language="{!objQuote.Proposal_Language__c}" readOnly="true" renderAs="pdf">
    <apex:stylesheet value="{!URLFOR($Resource.ServiceOrderCSS)}"/>    
    <apex:outputPanel rendered="{!$CurrentPage.parameters.internal == '1'}">
        <img src="{!$Resource.Watermark}" height="110%" width="110%" style="position: fixed;background-repeat:repeat;left: 1cm; top:2cm; z-index:0"/>
    </apex:outputPanel>
    <apex:variable var="endUserTag" value="{!IF(objQuote.BigMachines__Opportunity__r.PureConnect_Cloud_Partner_Approved__c, 'Participant', 'Customer')}"/>
    <table style="width:100%;">
        <tr>
            <td width="30%">
                <img src="{!$Resource.GenesysLogo}" width="200px;"/><br/>
                <apex:outputField value="{!objQuote.Selling_Entity__c}"/>
            </td>
           
            <td width="70%" style="text-align:right;font-size:10pt;">
                {!IF(objQuote.Order_Type__c == $Label.Amend_Subscription, 'Amendment','')} Services Order&nbsp;&nbsp;<apex:outputField value="{!objQuote.Name}"/>
                
            </td>
        </tr>
        <tr/><tr/>
        <tr>
            
            <td width="33%">
                <table border="1" style="border: 1px solid black;border-collapse:collapse;" align="left" width="90%">
                    <tr>
                        <td class="tableHeader">Bill To</td>
                        <td>
                            <apex:outputField value="{!objQuote.Sold_To_Name__r.Name}"/><br/>
                            <apex:outputPanel layout="none" rendered="{!list_QuoteAddresses.size>0}">                                
                                <apex:outputField value="{!list_QuoteAddresses[0].Address__c}"/><br/>
                                <apex:outputField value="{!list_QuoteAddresses[0].City__c}"/>&nbsp;
                                <apex:outputField value="{!list_QuoteAddresses[0].State__c}"/>&nbsp;
                                <apex:outputField value="{!list_QuoteAddresses[0].Zip_Code__c}"/><br/>
                                <apex:outputField value="{!list_QuoteAddresses[0].Country__c}"/> 
                            </apex:outputPanel>
                        </td>
                    </tr>
                    <tr>
                        <td class="tableHeader">End User</td>
                        <td>
                            <apex:outputField value="{!objQuote.End_User_ID__r.Name}"/> 
                        </td> 
                    </tr>
                </table>
            </td>
            <td width="33%">
                <table border="1" style="border: 1px solid black;border-collapse:collapse;" align="left" width="90%">
                    <tr> 
                        <td class="tableHeader">Ship To</td>
                        <td>
                            <apex:outputText value="{!IF(objQuote.BigMachines__Opportunity__r.Indirect_Partner_PureCloud_Approved__c, objQuote.Sold_To_Name__r.Name, objQuote.End_User_ID__r.Name)}"/><br/>
                            <apex:outputPanel layout="none" rendered="{!list_QuoteShipToAddresses.size>0}">                                
                                <apex:outputField value="{!list_QuoteShipToAddresses[0].Address__c}"/><br/>
                                <apex:outputField value="{!list_QuoteShipToAddresses[0].City__c}"/>&nbsp;
                                <apex:outputField value="{!list_QuoteShipToAddresses[0].State__c}"/>&nbsp;
                                <apex:outputField value="{!list_QuoteShipToAddresses[0].Zip_Code__c}"/><br/>
                                <apex:outputField value="{!list_QuoteShipToAddresses[0].Country__c}"/> 
                            </apex:outputPanel>
                        </td>
                    </tr>
                </table>  
            </td>
            <td width="33%">
                <table border="1" style="border: 1px solid black;border-collapse:collapse;text-align:center;" align="right" width="90%">
                    <tr>
                        <td class="tableHeader">Quote Expiration</td>
                        <td>
                            <apex:outputField value="{!objQuote.Expiration_Date__c}"/>
                        </td>
                    </tr>
                    <tr>
                        <td class="tableHeader">Date Created</td>
                        <td>
                            <apex:outputField value="{!objQuote.Proposal_Date__c}" rendered="{!objQuote.Proposal_Date__c != null}"/>
                            <apex:outputField value="{!objQuote.Quote_Created_Date_Formula__c}" rendered="{!objQuote.Proposal_Date__c == null}"/>
                        </td>
                    </tr>
                    <tr style="{!IF(objQuote.Order_Type__c==$Label.Renew_Subscription, '', 'display:none')}">
                        <td class="tableHeader">Start Date</td>
                        <td>
                            <apex:outputField value="{!objQuote.Subscription_Start_Date__c}" rendered="{!(NOT(ISBLANK(objQuote.Subscription_Start_Date__c)) && objQuote.Subscription_Start_Date__c >= TODAY())}"/>
                            <apex:outputText value="{!MONTH(TODAY())}/{!DAY(TODAY())}/{!YEAR(TODAY())}" rendered="{!objQuote.Subscription_Start_Date__c < TODAY()}"/>
                        </td>
                    </tr>
                </table>
            </td>
        </tr>
        <tr/><tr/>
        <tr>
            <td>
                <table border="1" style="border: 1px solid black;border-collapse:collapse;text-align:center;" align="left" width="90%">
                    <tr>
                        <td class="tableHeader">Account Executive</td>
                        <td class="tableHeader">Customer Contact</td>
                    </tr>
                    <tr>
                        <td>
                            <apex:outputField value="{!objQuote.Owner.Name}"/>
                        </td>
                        <td>
                            <apex:outputField value="{!objQuote.Contact__r.Name}"/>
                        </td>
                    </tr>
                </table>
            </td>
            <td>
                <table border="1" style="border: 1px solid black;border-collapse:collapse;text-align:center;" align="center" width="90%">
                    <tr>
                        <td class="tableHeader">Phone</td>
                        <td>
                            <apex:outputField value="{!objQuote.Contact__r.ContactPhone__c}"/>
                        </td>
                    </tr>
                    <tr>
                        <td class="tableHeader">E-mail</td>
                        <td>
                            <apex:outputText value="{!objQuote.Contact__r.Email}"/>
                        </td>
                    </tr>
                </table>
            </td>
            <td>
                <apex:outputPanel layout="none" rendered="{!blnHasRecurring}">
                    <table border="1" style="border: 1px solid black;border-collapse:collapse;text-align:center;" align="right" width="90%">
                        <tr>
                            <td class="tableHeader">{!IF(objQuote.Order_Type__c == $Label.Renew_Subscription, '', 'Initial')} Subscription Term</td>
                            <td class="tableHeader">Renewal Subscription Term</td>
                            <td class="tableHeader" style="{!IF(objQuote.Order_Type__c == $Label.Amend_Subscription, '', 'display:none')}">Remaining Term</td>
                        </tr>
                        <tr>
                            <td>
                                {!IF(objQuote.Initial_Subscription_Term__c == 'Other',TEXT(objQuote.No_Of_Months__c) & ' Months',objQuote.Initial_Subscription_Term__c)}
                            </td>
                            <td>
                                <apex:outputField value="{!objQuote.Renewal_Subscription_Term__c}"/>
                            </td>
                            <td style="{!IF(objQuote.Order_Type__c == $Label.Amend_Subscription, '', 'display:none')}">
                                <apex:outputText value="{!intRemainingTerm}"/>
                            </td>
                        </tr>
                    </table>
                </apex:outputPanel>
            </td>
        </tr>
        <tr/><tr/>
        <tr>
            <td>
                <table border="1" style="border: 1px solid black;border-collapse:collapse;text-align:center;" align="left" width="90%">
                    <tr>
                        <td class="tableHeader">Product Tier</td>
                        <td class="tableHeader">Pricing Model</td>
                        <td class="tableHeader">Ramp Period</td>
                        <td class="tableHeader">Deployment Model</td>
                    </tr>
                    <tr>
                        <td>
                            <apex:outputField value="{!objQuote.Product_Tier__c}"/>
                        </td>
                        <td>
                            <apex:outputField value="{!objQuote.Pricing_Model__c}"/>
                        </td>
                        <td>
                            <apex:outputField value="{!objQuote.Ramp_Period__c}"/>
                        </td>
                        <td>
                            <apex:outputField value="{!objQuote.Deployment_Model__c}"/>
                        </td> 
                    </tr>
                </table>
            </td>
            <td colspan="4">
                <table border="1" style="border: 1px solid black;border-collapse:collapse;text-align:center;" align="right" width="90%">
                    <tr>
                        <td class="tableHeader">Payment Terms</td>
                        <td class="tableHeader">Billing Period</td>
                        <td class="tableHeader">Currency</td>
                        <!--  <td class="tableHeader">Min. Period Fees</td> -->
                    </tr>
                    <tr>
                        <td>
                            <apex:outputField value="{!objQuote.Order_Payment_Terms__c}"/>
                        </td>
                        <td>
                            <apex:outputField value="{!objQuote.Billing_Period__c}"/>
                        </td>
                        <td>
                            <apex:outputField value="{!objQuote.CurrencyISOCode}"/>
                        </td>
                        <!--    <td>
<apex:outputField value="{!objQuote.CMRR__c}"/>
</td>-->
                    </tr>
                    <tr width="100%">
                        <td class="tableHeader">Burst Allowance %</td>
                        <td class="tableHeader" colspan="2">Max Annual Price Increase %</td>
                    </tr> 
                    
                    <tr width="100%">
                        <td width="50%" >
                            <apex:outputText value="{!IF(objQuote.Overage_Allowance__c == 'Other',objQuote.Overage_Allowance_Other__c,objQuote.Overage_Allowance__c)}"/>
                        </td>
                        <td colspan="2">
                            <apex:outputText value="{!IF(objQuote.Price_Escalation__c == 'Other',objQuote.Annual_Price_Escalation_Other__c,objQuote.Price_Escalation__c)}"/>
                        </td> 
                    </tr>
                </table>
            </td>
        </tr>
    </table>
    <br/><br/><br/>
    
    <apex:repeat value="{!list_ProductGroups}" var="objProductGroup">
        
        <apex:outputPanel layout="none" rendered="true">
            <apex:outputPanel rendered="{!objProductGroup.Group_Name__c = 'Software Licensing' && blnHasRecurring && NOT(objQuote.Show_One_Time_Fees_Only__c)}">
                <apex:outputText value="Subscription Fees:" styleClass="mainHeader"/>
                <br/><br/>
            </apex:outputPanel>
            <apex:outputPanel layout="none" rendered="{!objProductGroup.Group_Name__c =='Bursting Premium Pricing' &&  list_OverageProducts.size > 0 && NOT(objQuote.Show_One_Time_Fees_Only__c)}">
                <apex:outputPanel >
                    <apex:outputText value="{!objProductGroup.Group_Name__c}" styleClass="mainHeader" />
                    <br/><br/>
                </apex:outputPanel>
            </apex:outputPanel>    
            <apex:outputPanel rendered="{!(objProductGroup.Group_Name__c = 'Monthly Telco/MPLS/Data Center Fees' || objProductGroup.Group_Name__c = 'Monthly Telco') && blnHasMonthlyTelco && NOT(objQuote.Show_One_Time_Fees_Only__c)}">
                <apex:outputText value="Monthly Recurring Telco/MPLS/Data Center Fees:" styleClass="mainHeader"/>
                <br/><br/>
            </apex:outputPanel>
            <apex:outputPanel rendered="{!objProductGroup.Group_Name__c == 'Upfront Provisioning Fees' && blnHasNonRecurring}">
                <apex:outputText value="Non-Recurring Fees:" styleClass="mainHeader"/>
                <br/><br/>
            </apex:outputPanel>
        </apex:outputPanel>
        
        <apex:outputPanel rendered="{!(map_ProductGroupsToShow[objProductGroup.Group_Name__c] && (objProductGroup.Billing_Type__c =='Non-Recurring Fees' || (objProductGroup.Billing_Type__c != 'Non-Recurring Fees' && NOT(objQuote.Show_One_Time_Fees_Only__c))))}">
            <table cellspacing="1" cellpadding="5" border="1" style="border: 1px solid black;border-collapse:collapse;page-break-before:auto;page-break-inside:avoid;" width="100%">
                <tr class="tableHeader" style="font-size:9pt;font-family:'Calibri';">
                    <td colspan="15">{!objProductGroup.Header_Text__c}</td>
                </tr> 
                <tr style="font-weight:bold;text-align:center;">
                    <th width="20%">Product</th>
                    <th width="30%">Description</th>
                    <th width="5%" style="{!IF(objQuote.Order_Type__c == $Label.Amend_Subscription && objProductGroup.Billing_Type__c != 'Non-Recurring Fees' , '', 'display:none')}">Original<br/>Quantity</th>
                    <th width="5%">{!IF(objQuote.Order_Type__c == $Label.Amend_Subscription && objProductGroup.Billing_Type__c != 'Non-Recurring Fees', 'Updated Quantity', 'Quantity')}</th>
                    <apex:outputPanel layout="none" rendered="{!NOT(objQuote.Hide_List_Price__c)}">
                        <th>List Price</th>
                    </apex:outputPanel>
                    <th width="10%" style="{!IF(objQuote.BigMachines__Opportunity__r.PureConnect_Cloud_Partner_Approved__c, '', 'display:none')}">Partner Price</th>
                    <apex:outputPanel layout="none" rendered="{!NOT(objQuote.Hide_Discount__c)}">
                        <th>Discount (%)</th>
                    </apex:outputPanel>
                    <th>Net Unit Price</th>  
                    <th width="12%">{!IF(objQuote.Order_Type__c == $Label.Amend_Subscription, 'Updated Ext Price', 'Ext Price')}<br/>{!IF(objProductGroup.Billing_Type__c == 'Non-Recurring Fees', '', IF(objProductGroup.Billing_Type__c == 'Monthly Recurring (Telco) Fees' , '(Monthly)', IF(objProductGroup.Billing_Type__c == 'Bursting Premium Pricing','(Monthly)',IF(objProductGroup.Billing_Type__c == 'Recurring Fees', '('+strRecServBillingPeriod+')', '('+objQuote.Billing_Period__c+')'))))} </th>
                </tr>
                <!--<apex:outputText value="{!map_ExistingOriginalSubscrProds2}"/>-->
                <apex:repeat value="{!map_ProductGroupToQuoteProducts[objProductGroup.Group_Name__c]}" var="objQP" rendered="{!objProductGroup.Billing_Type__c == 'Non-Recurring Fees'|| (objProductGroup.Billing_Type__c != 'Non-Recurring Fees' && NOT(objQuote.Show_One_Time_Fees_Only__c))}">
                    <apex:variable var="uniqueKey" value=""/>
                    <apex:variable var="orgQty" value=""/>
                    <apex:variable value="{!objQP.BigMachines__Product__c};{!objQP.Is_Overage__c}" var="uniqueKey" rendered="{!objQP.Subscription_Product__c != null}"/>
                    <apex:variable value="{!if(uniqueKey != null, map_ExistingOriginalSubscrProds[uniqueKey].Product_Quantity__c, null)}" var="orgQty" rendered="{!((objQuote.Order_Type__c == $Label.Amend_Subscription || objQuote.Order_Type__c == $Label.Renew_Subscription)&&uniqueKey != null)}"/>
                    <!--<apex:variable value="{!if(map_ExistingOriginalSubscrProds[uniqueKey] != null, map_ExistingOriginalSubscrProds[uniqueKey].Ext_Net_Price__c, null)}" var="orgExtPrice" rendered="{!((objQuote.Order_Type__c == $Label.Amend_Subscription || objQuote.Order_Type__c == $Label.Renew_Subscription))}"/>-->
                    
                    <tr style="text-align:center;">
                        <td><apex:outputText value="{!if(contains(objQP.Product_Group__c,'PureSuccess'),objQP.BigMachines__Product__r.description__c+' ('+objQP.BigMachines__Product__r.APN_Number__c+') ',objQP.Name+' ('+objQP.BigMachines__Product__r.APN_Number__c+') ')}"/></td>
                        <td><apex:outputText value="{!objQP.BigMachines__Product__r.Description}"/></td>
                        <td style="{!IF((objQuote.Order_Type__c == $Label.Amend_Subscription && objProductGroup.Billing_Type__c != 'Non-Recurring Fees' && intSizeofExistingOriginalSubscrProds > 0 ), '', 'display:none')}">
                            <apex:outputText value="{!IF(orgQty != null,IF(len(orgQty)>0,orgQty,0),0)}"/>
                        </td>    
                        <td><apex:outputText value="{!IF((objQuote.Order_Type__c == $Label.Amend_Subscription || objQuote.Order_Type__c == $Label.Renew_Subscription) && objProductGroup.Billing_Type__c == 'Non-Recurring Fees',IF(contains($Label.Check_For_One_Time_Fees_PCC, objQP.BigMachines__Product__r.New_Category__c),objQP.Quantity_Change__c,objQP.Updated_Quantity__c),objQP.Product_Quantity__c)}"/></td>                         
                        <!-- Anju PCC Amend && objProductGroupMain != 'Non-Recurring Fees' && objProductGroupMain=='Non-Recurring Fees'-->
                        <apex:outputPanel layout="none" rendered="{!NOT(objQuote.Hide_List_Price__c)}">
                            <td>{!strCurrencySymbol}
                                <!--Mukunth added below code for 6 digit project on 10-APR-->
                                <!--<apex:outputText value="{0, number, ###,##0.00}">
<apex:param value="{!objQP.List_Price__c}"/>
</apex:outputText>-->
                                <apex:outputText value="{0, number,###,###,##0.00}" rendered="{!mapProdToPrecision[objQP.BigMachines__Product__c].Tier_Pricing_Decimal_Precision__c == null || mapProdToPrecision[objQP.BigMachines__Product__c].Tier_Pricing_Decimal_Precision__c == '2'}">
                                    <apex:param value="{!objQP.List_Price__c}"/>
                                </apex:outputText>
                                <apex:outputText value="{0, number,###,###,##0.0000}" rendered="{!mapProdToPrecision[objQP.BigMachines__Product__c].Tier_Pricing_Decimal_Precision__c == '4'}">
                                    <apex:param value="{!objQP.List_Price__c}"/>
                                </apex:outputText>
                                <apex:outputText value="{0, number,###,###,##0.000000}" rendered="{!mapProdToPrecision[objQP.BigMachines__Product__c].Tier_Pricing_Decimal_Precision__c == '6'}">
                                    <apex:param value="{!objQP.List_Price__c}"/>
                                </apex:outputText> 
                                <!--Mukunth code end-->
                            </td>
                        </apex:outputPanel>
                        <td style="{!IF(objQuote.BigMachines__Opportunity__r.PureConnect_Cloud_Partner_Approved__c, '', 'display:none')}">
                            <!--Mukunth added below code for 6 digit project on 10-APR-->
                            <!--<apex:outputText value="{!strCurrencySymbol}{0, number, ###,##0.00}">
<apex:param value="{!objQP.Partner_Price__c}"/>
</apex:outputText>-->
                            <apex:outputText value="{!strCurrencySymbol}{0, number,###,###,##0.00}" rendered="{!mapProdToPrecision[objQP.BigMachines__Product__c].Tier_Pricing_Decimal_Precision__c == null || mapProdToPrecision[objQP.BigMachines__Product__c].Tier_Pricing_Decimal_Precision__c == '2'}">
                                <apex:param value="{!objQP.Partner_Price__c}"/>
                            </apex:outputText>
                            <apex:outputText value="{!strCurrencySymbol}{0, number,###,###,##0.0000}" rendered="{!mapProdToPrecision[objQP.BigMachines__Product__c].Tier_Pricing_Decimal_Precision__c == '4'}">
                                <apex:param value="{!objQP.Partner_Price__c}"/>
                            </apex:outputText>
                            <apex:outputText value="{!strCurrencySymbol}{0, number,###,###,##0.000000}" rendered="{!mapProdToPrecision[objQP.BigMachines__Product__c].Tier_Pricing_Decimal_Precision__c == '6'}">
                                <apex:param value="{!objQP.Partner_Price__c}"/>
                            </apex:outputText>
                            <!--Mukunth code end-->
                        </td>
                        <apex:outputPanel layout="none" rendered="{!NOT(objQuote.Hide_Discount__c)}">
                            <td><apex:outputField value="{!objQP.Total_Discount__c}"/></td>
                        </apex:outputPanel>
                        <td>{!strCurrencySymbol}
                            <!--Mukunth added below code for 6 digit project on 10-APR-->
                            <!--<apex:outputText value="{0, number, ###,##0.00}">
<apex:param value="{!objQP.Net_Unit_Price__c}"/>
</apex:outputText>-->
                            <apex:outputText value="{0, number,###,###,##0.00}" rendered="{!mapProdToPrecision[objQP.BigMachines__Product__c].Tier_Pricing_Decimal_Precision__c == null || mapProdToPrecision[objQP.BigMachines__Product__c].Tier_Pricing_Decimal_Precision__c == '2'}">
                                <apex:param value="{!objQP.Net_Unit_Price__c}"/>
                            </apex:outputText>
                            <apex:outputText value="{0, number,###,###,##0.0000}" rendered="{!mapProdToPrecision[objQP.BigMachines__Product__c].Tier_Pricing_Decimal_Precision__c == '4'}">
                                <apex:param value="{!objQP.Net_Unit_Price__c}"/>
                            </apex:outputText>
                            <apex:outputText value="{0, number,###,###,##0.000000}" rendered="{!mapProdToPrecision[objQP.BigMachines__Product__c].Tier_Pricing_Decimal_Precision__c == '6'}">
                                <apex:param value="{!objQP.Net_Unit_Price__c}"/>
                            </apex:outputText> 
                            <!--Mukunth code end-->
                        </td>
                        <td>{!strCurrencySymbol}
                            <apex:outputText value="{0, number, ###,##0.00}">
                                <apex:param value="{!IF((objQuote.Order_Type__c == $Label.Amend_Subscription) && objProductGroup.Billing_Type__c =='Non-Recurring Fees', (objQP.Quantity_Change__c * objQP.Net_Unit_Price__c),IF(contains(map_ProdGroupCategorization['Monthly Recurring (Telco) Fees'], objProductGroup.Group_Name__c), objQP.Extended_Price_Per_Month__c, objQP.Extended_Price_Per_Period__c))}"/>
                            </apex:outputText>
                        </td>
                    </tr>
                </apex:repeat>
            </table>
            <br/><br/>
            <!--Added By Ranjith on May 24, 2017 -- Starts -->
            <apex:outputPanel rendered="{!blnDisplayMonthlyTelcoTable && (objProductGroup.Header_Text__c='Monthly Telco/MPLS/Data Center Fees' || objProductGroup.Group_Name__c='Monthly Telco')}">
                <table cellspacing="1" cellpadding="5" border="1" style="border: 1px solid black;border-collapse:collapse;page-break-before:auto;page-break-inside:avoid;" width="100%">
                    <tr>
                        <td>
                            Genesys will provide {!objMonthlyTelcoProd.Product_Quantity__c} Telco Channels within the primary PureConnect Cloud Data Center to support your call traffic to and from the PSTN.  An equal number of lines will be reserved within the secondary data center to serve as back up.
                        </td>
                        <td style="text-align:right;">
                            Customer will pay {!strCurrencySymbol}{!objMonthlyTelcoProd.Ext_Net_Price__c} per month for {!objMonthlyTelcoProd.Product_Quantity__c} Telco dedicated Telco Channels within the primary Data Center and an equal number used for back up within the secondary data center.<br/><br/>
                            Genesys will provide access to an additional {!objMonthlyTelcoProd.Product_Quantity__c} Telco Channels for use as needed, and when available.  Customer will be invoiced<br/>{!strCurrencySymbol}{!ROUND(objMonthlyTelcoProd.List_Price__c, 2)} per month for each additional Telco Channel utilized.
                        </td>
                    </tr>
                    <tr class="tableHeader" style="font-size:9pt;font-family:'Calibri';text-align:center;">
                        <td>Telco Usage</td>
                        <td>Costs</td>
                    </tr>
                    <tr>
                        <td>Domestic U.S. Outbound and Inbound Call Traffic established over the PSTN. (CS-012-NV-AR11)</td>
                        <td style="text-align:right;">{!strCurrencySymbol}{!ROUND(0.012*conversionRate, 3)} per minute billed in one second increments.</td>
                    </tr>
                    <tr>
                        <td>Intrastate Call Traffic:</td>
                        <td style="text-align:right;">{!strCurrencySymbol}{!ROUND(0.0151*conversionRate, 4)} per minute billed in one second increments.</td>
                    </tr>
                    <tr>
                        <td>International Call Traffic:(CS-012-NV-AR12)</td>
                        <td style="text-align:right;">International calls are billed at the usage rates in effect at the time of the call.  An International Usage Rate Schedule will be provided upon request.</td>
                    </tr>
                    <tr class="tableHeader" style="font-size:9pt;font-family:'Calibri';text-align:center;">
                        <td>Assigned Phone Numbers</td>
                        <td>Costs</td>
                    </tr>
                    <tr>
                        <td>Toll Free Phone Numbers: (CS-012-NV-AR04)</td>
                        <td style="text-align:right;">{!strCurrencySymbol}{!ROUND(2.50*conversionRate, 2)} per Toll Free Phone Number, per Month</td>
                    </tr>
                    <tr>
                        <td>Direct Inward Dial Phone Numbers: (CS-012-NV-AR06)</td>
                        <td style="text-align:right;">{!strCurrencySymbol}{!ROUND(1.50*conversionRate, 2)} per Direct Inward Dial Phone Number, per Month</td>
                    </tr>
                    <tr>
                        <td colspan="2" style="font-weight:bold;font-style:italic;text-align:center;">Telco Pricing excludes taxes, surcharges and tariffs, which are billed separately as incurred.</td>
                    </tr>
                </table>
            </apex:outputPanel>
            <br/><br/>
            <!--Added By Ranjith on May 24, 2017 -- Ends -->
        </apex:outputPanel>
    </apex:repeat>
    
    <!-- Anju PCC Amend -->
