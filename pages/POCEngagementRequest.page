       function CheckFnUseCaseInput()
        {
          var isReturn = true;
          var arrLinks = j$('.clsFNUseCaseInput');
          var arrAsIs = j$('.clsAsIsFnUseCase');
          var arrDesc = j$('.clsDescFnUseCase');
          
          for(var i=0;i<arrLinks.length;i++)
          {
            if(j$(arrLinks[i]).val() == '')
              isReturn = false;
          }
          if(isReturn == false)
           alert('Please select functional use case.');
           
          for(var i=0;i<arrLinks.length;i++)
          {
              if(j$(arrAsIs[i]).val() == '100')
                  j$(arrDesc[i]).val('');
             //if(j$(arrLinks[i]).val() != '' && j$(arrAsIs[i]).is(':checked') == false && j$(arrDesc[i]).val() == '')
             if(j$(arrLinks[i]).val() != '' && j$(arrAsIs[i]).val() != '100' && j$(arrDesc[i]).val() == '')
             {
               isReturn = false;
               alert('Please fill short description of changes in Functional Use Case.');
             }
          }
           
          return isReturn; 
        }
        
        
        function CheckFnUseCase(obj)
