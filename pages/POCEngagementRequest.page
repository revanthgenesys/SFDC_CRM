    function VisibilityVirtualSection(sType)
        {
          if(sType == 'Reject')
          {
            var objChildArraySE = j$('.clsSolutionCon').children('.lookupInput').children();
            j$(objChildArraySE[2]).hide();
            j$(objChildArraySE[0]).attr('disabled',true);
            j$('.clsSolutionConType').attr('disabled',true);
            j$(objChildArraySE[0]).val('');
            j$('.clsSolutionConType').val('');
            j$('[id$= btnAddVTUsers]').hide();
          }
          if(sType == 'Approve')
          {
            var objChildArraySE = j$('.clsSolutionCon').children('.lookupInput').children();
            j$(objChildArraySE[2]).show();
            j$(objChildArraySE[0]).attr('disabled',false);
            j$('.clsSolutionConType').attr('disabled',false);
            j$('[id$= btnAddVTUsers]').show();
          }
        }

