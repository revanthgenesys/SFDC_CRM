     var VTUsersList = j$('[id$=hdnVirUsr]');//array of users
     var selectUserVT = j$('.clsSolutionCon').children('[id*=lkid]').val();//single user
     for(var i=0;i<VTUsersList.length;i++)
     {
       if(j$(VTUsersList[i]).val().trim().indexOf(selectUserVT.trim())>-1)
       {
         // selected user alreday exist in the Virtual List
         isExist= false;
         alert('Selected user already present in the list.');
       }
     }       
      return isExist;
    }
       
    
