<apex:page controller="ProposalCloudPECController" applyBodyTag="false" standardStylesheets="false"
           language="{!objQuote.Proposal_Language__c}" readOnly="true" renderas="pdf">
    <apex:stylesheet value="{!URLFOR($Resource.ServiceOrderCSS)}" />
    <apex:outputPanel rendered="{!$CurrentPage.parameters.internal == '1'}">
        <img src="{!$Resource.Watermark}" height="110%" width="110%" style="position: fixed;background-repeat:repeat;left: 1cm; top:2cm; z-index:0"
             />
    </apex:outputPanel>
    <apex:variable var="endUserTag" value="{!IF(objQuote.BigMachines__Opportunity__r.Indirect_Partner_PureCloud_Approved__c, 'Participant', 'Customer')}"
                   />
    <table style="width:100%;">
        <tr>
            <td width="30%">
                <img src="{!$Resource.GenesysLogo}" width="200px;" /><br/>
                <apex:outputField value="{!objQuote.Selling_Entity__c}" />
            </td>
            <td width="70%" style="text-align:right;font-size:10pt;">
                {!IF(objQuote.Order_Type__c == $Label.Amend_Subscription, 'Amendment','')} Services Order&nbsp;&nbsp;<apex:outputField value="{!objQuote.Name}"/>
            </td>
        </tr>
        <tr/>
        <tr/>
        <tr>
            
            <td width="33%">
                <table border="1" style="border: 1px solid black;border-collapse:collapse;" align="left"
                       width="90%">
                    <tr>
                        <td class="tableHeader">Bill To</td>
                        <td>
                            <apex:outputText value="{!IF(objQuote.BigMachines__Opportunity__r.Indirect_Partner_PureCloud_Approved__c, objQuote.Sold_To_Name__r.Name, objQuote.End_User_ID__r.Name)}"
                                             /><br/>
                            <apex:outputPanel layout="none" rendered="{!list_QuoteAddresses.size>0}">
                                <apex:outputField value="{!list_QuoteAddresses[0].Address__c}" /><br/>
                                <apex:outputField value="{!list_QuoteAddresses[0].City__c}" />&nbsp;
                                <apex:outputField value="{!list_QuoteAddresses[0].State__c}" />&nbsp;
                                <apex:outputField value="{!list_QuoteAddresses[0].Zip_Code__c}" /><br/>
                                <apex:outputField value="{!list_QuoteAddresses[0].Country__c}" />
                            </apex:outputPanel>
                        </td>
                    </tr>
                    <tr>
                        <td class="tableHeader">End User</td>
                        <td>
                            <apex:outputField value="{!objQuote.End_User_ID__r.Name}" />
                        </td>
                    </tr>
                </table>
            </td>
            <td width="33%">
                <table border="1" style="border: 1px solid black;border-collapse:collapse;" align="left" width="90%">
                    <tr> 
                        <td class="tableHeader">Ship To</td>
                        <td>
                            <apex:outputText value="{!IF(objQuote.BigMachines__Opportunity__r.Indirect_Partner_PureCloud_Approved__c, objQuote.Sold_To_Name__r.Name, objQuote.End_User_ID__r.Name)}"/><br/>
                            <apex:outputPanel layout="none" rendered="{!list_QuoteShipToAddresses.size>0}">                                
                                <apex:outputField value="{!list_QuoteShipToAddresses[0].Address__c}"/><br/>
                                <apex:outputField value="{!list_QuoteShipToAddresses[0].City__c}"/>&nbsp;
                                <apex:outputField value="{!list_QuoteShipToAddresses[0].State__c}"/>&nbsp;
                                <apex:outputField value="{!list_QuoteShipToAddresses[0].Zip_Code__c}"/><br/>
                                <apex:outputField value="{!list_QuoteShipToAddresses[0].Country__c}"/> 
                            </apex:outputPanel>
                        </td>
                    </tr>
                </table>  
            </td>
            <td width="33%">
                <table border="1" style="border: 1px solid black;border-collapse:collapse;text-align:center;"
                       align="right" width="90%">
                    <tr>
                        <td class="tableHeader">Quote Expiration</td>
                        <td>
                            <apex:outputField value="{!objQuote.Expiration_Date__c}" />
                        </td>
                    </tr>
                    <tr>
                        <td class="tableHeader">Date Created</td>
                        <td>
                            <apex:outputField value="{!objQuote.Proposal_Date__c}" rendered="{!objQuote.Proposal_Date__c != null}"
                                              />
                            <apex:outputField value="{!objQuote.Quote_Created_Date_Formula__c}" rendered="{!objQuote.Proposal_Date__c == null}"
                                              />
                        </td>
                    </tr>
                    <tr style="{!IF(objQuote.Order_Type__c==$Label.Renew_Subscription, '', 'display:none')}">
                        <td class="tableHeader">Start Date</td>
                        <td>
                            <apex:outputField value="{!objQuote.Subscription_Start_Date__c}" rendered="{!objQuote.Subscription_Start_Date__c >= TODAY()}"
                                              />
                            <apex:outputText value="{!MONTH(TODAY())}/{!DAY(TODAY())}/{!YEAR(TODAY())}" rendered="{!objQuote.Subscription_Start_Date__c < TODAY()}"
                                             />
                        </td>
                    </tr>
                </table>
            </td>
        </tr>
        <tr/>
        <tr/>
        <tr>
            <td>
                <table border="1" style="border: 1px solid black;border-collapse:collapse;text-align:center;"
                       align="left" width="90%">
                    <tr>
                        <td class="tableHeader">Account Executive</td>
                        <td>
                            <apex:outputField value="{!objQuote.Owner.Name}" />
                        </td>
                        
                    </tr>
                    <tr>
                        <td class="tableHeader">Customer Contact</td>
                        <td>
                            <apex:outputField value="{!objQuote.Contact__r.Name}" />
                        </td>
                    </tr>
                </table>
            </td>
            <td>
                <table border="1" style="border: 1px solid black;border-collapse:collapse;text-align:center;"
                       align="center" width="90%">
                    <tr>
                        <td class="tableHeader">Phone</td>
                        <td>
                            <apex:outputText value="{!IF(objQuote.Contact__r.Phone != '', objQuote.Contact__r.Phone, objQuote.Contact__r.ContactPhone__c)}"
                                             />
                        </td>
                    </tr>
                    <tr>
                        <td class="tableHeader">E-mail</td>
                        <td>
                            <apex:outputText value="{!objQuote.Contact__r.Email}" />
                        </td>
                    </tr>
                </table>
            </td>
        </tr>
        <tr/>
        <td colspan="4">
            <table border="1" style="border: 1px solid black;border-collapse:collapse;text-align:center;"
                   align="center" width="100%">
                <tr>
                    <td class="tableHeader">{!IF(objQuote.Order_Type__c==$Label.Renew_Subscription, 'Extended',
                        'Initial')} Subscription Term</td>
                    <td class="tableHeader">Renewal Subscription Term</td>
                    <td class="tableHeader" style="{!IF(objQuote.Order_Type__c == $Label.Amend_Subscription, '', 'display:none')}">Remaining Term</td>
                    <td class="tableHeader" style="{!IF(objQuote.Order_Type__c == $Label.New_Subscription || objQuote.Order_Type__c == $Label.Replace_Subscription, '', 'display:none')}">Ramp Period (Months)</td>
                </tr>
                <tr>
                    <td>
                        {!IF(objQuote.Initial_Subscription_Term__c == 'Other',TEXT(objQuote.No_Of_Months__c)
                        & ' Months', objQuote.Initial_Subscription_Term__c)}
                    </td>
                    <td>
                        {!objQuote.Renewal_Subscription_Term__c}
                    </td>
                    <td style="{!IF(objQuote.Order_Type__c == $Label.Amend_Subscription, '', 'display:none')}">
                        <apex:outputText value="{!intRemainingTerm}" />
                    </td>
                    <td style="{!IF(objQuote.Order_Type__c == $Label.New_Subscription || objQuote.Order_Type__c == $Label.Replace_Subscription, '', 'display:none')}">
                        <apex:outputText value="{!IF(objQuote.Ramp_Period__c !='1 Year',VALUE(objQuote.Ramp_Period__c)/30,'12')}"/>
                    </td>
                </tr>
            </table>
        </td>
        <tr/>
        <tr>
            <td>
                <table border="1" style="border: 1px solid black;border-collapse:collapse;text-align:center;"
                       align="left" width="90%">
                    <tr>
                        <td class="tableHeader">Currency</td>
                        <td class="tableHeader">Period</td>
                    </tr>
                    <tr>
                        <td>
                            <apex:outputField value="{!objQuote.CurrencyIsoCode}" />
                        </td>
                        <td>
                            <apex:outputField value="{!objQuote.Period__c}" />
                        </td>
                    </tr>
                </table>
            </td>
            <td>
                <table border="1" style="border: 1px solid black;border-collapse:collapse;text-align:center;"
                       align="center" width="100%">
                    <tr>
                        <td class="tableHeader">Payment Terms</td>
                        <td class="tableHeader">Billing Period</td>
                        
                    </tr>
                    <tr>
                        <td>
                            <apex:outputField value="{!objQuote.Order_Payment_Terms__c}"
                                              />
                        </td>
                        <td>
                            <apex:outputField value="{!objQuote.Billing_Period__c}" />
                        </td>
                        
                    </tr>
                    <tr width="100%">
                        <td class="tableHeader">Burst Allowance %</td>
                        <td class="tableHeader" colspan="2">Max Annual Price Increase %</td>
                    </tr>
                    
                    <tr width="100%">
                        <td width="50%">
                            <apex:outputText value="{!IF(objQuote.Overage_Allowance__c == 'Other',objQuote.Overage_Allowance_Other__c,objQuote.Overage_Allowance__c)}"
                                             />
                        </td>
                        <td colspan="2">
                            <apex:outputText value="{!IF(objQuote.Price_Escalation__c == 'Other',objQuote.Annual_Price_Escalation_Other__c,objQuote.Price_Escalation__c)}"
                                             />
                        </td>
                    </tr>
                    
                </table>
            </td>
            
            <td>
                <table border="1" style="border: 1px solid black;border-collapse:collapse;text-align:center;"
                       align="center" width="90%">
                    <tr>
                        <td class="tableHeader">Pricing Model</td>
                    </tr>
                    <tr>
                        <td>
                            <apex:outputText value="{!objQuote.Pricing_Model__c}" />
                        </td>
                    </tr>
                    <tr>
                        <td class="tableHeader">Contract terms drive ACV Change?</td>
                    </tr>
                    <tr>
                        <td>
                            <apex:outputText value="{!objQuote.Contract_terms_drive_ACV_Change__c}" />
                        </td>
                    </tr>
                </table>
            </td>
        </tr>
    </table>
    <br/><br/><br/>
    
    <apex:repeat value="{!list_OrderedHeadings}" var="sortOrder">
        <apex:variable value="{!map_HeadingsWithSortOrder[sortOrder]}" var="objProductGroupMain"
                       />
        <apex:outputPanel layout="none" rendered="{!objProductGroupMain=='Non Recurring Fees' || (objProductGroupMain != 'Non Recurring Fees' && NOT(objQuote.Show_One_Time_Fees_Only__c)) && IF(CONTAINS(strProductGroupKey,objProductGroupMain),true,false)}">
            <!-- rendered="{!objProductGroupMain=='Non Recurring Fees' || (objProductGroupMain != 'Non Recurring Fees' && NOT(objQuote.Show_One_Time_Fees_Only__c)) && IF(CONTAINS(strProductGroupKey,objProductGroupMain),true,false)}" -->
            <apex:outputPanel >
                <apex:outputText value="{!objProductGroupMain}" styleClass="subHeader" />
                <br/><br/>
            </apex:outputPanel>
        </apex:outputPanel>
        <apex:repeat value="{!map_ProdGrpToQtProds[objProductGroupMain]}" var="objProductGroup"
                     rendered="{!((objProductGroupMain=='Non Recurring Fees' || (objProductGroupMain != 'Non Recurring Fees' && NOT(objQuote.Show_One_Time_Fees_Only__c))) && IF(CONTAINS(strProductGroupKey,objProductGroupMain),true,false))}">
            <table cellspacing="1" cellpadding="5" border="1" style="border: 1px solid black;border-collapse:collapse;page-break-before:auto;page-break-inside:avoid;"
                   width="100%">
                <tr class="tableHeader" style="font-size:9pt;font-family:'Calibri';">
                    <td colspan="{!4+IF(objQuote.Order_Type__c == $Label.Amend_Subscription && objProductGroupMain != 'Non Recurring Fees',1,0)+IF(objQuote.Hide_List_Price__c,0,1)+IF(objQuote.BigMachines__Opportunity__r.Indirect_Partner_PureCloud_Approved__c,1,0)+IF(objQuote.Hide_Discount__c,0,1) + IF(objQuote.BigMachines__Opportunity__r.Offering_Detail__c != 'Free Trial',1,0)}">{!objProductGroup}</td>
                </tr>
                <tr style="font-weight:bold;text-align:center;">
                    <th width="10%">Product</th>
                    <th width="10%">Unit</th>
                    <th width="5%" style="{!IF(objQuote.Order_Type__c == $Label.Amend_Subscription && objProductGroupMain != 'Non Recurring Fees', '', 'display:none')}">Original<br/>Quantity</th>
                    <th width="5%">{!IF(objQuote.Order_Type__c == $Label.Amend_Subscription && objProductGroupMain
                        != 'Non Recurring Fees', 'Updated Quantity', 'Quantity')}</th>
                    <th width="10%" style="{!IF(objQuote.Hide_List_Price__c, 'display:none', '')}">List Price</th>
                    <th width="10%" style="{!IF(objQuote.BigMachines__Opportunity__r.Indirect_Partner_PureCloud_Approved__c, '', 'display:none')}">Partner Price</th>
                    <th width="9%" style="{!IF(objQuote.Hide_Discount__c, 'display:none', '')}">Discount (%)</th>
                    <th width="10%">Net Unit Price</th>
                    <th width="12%">{!IF(objQuote.Order_Type__c == $Label.Amend_Subscription, 'Updated
                        Ext Price', 'Ext Price')} {!IF(objProductGroupMain=='Non
                        Recurring Fees', '', IF(objProductGroupMain=='Overage Subscription
                        Pricing', '(Monthly)', '('+objQuote.Billing_Period__c+')'))}</th>
                </tr>
                
                <apex:repeat value="{!map_ProdGrpToQtProds[objProductGroupMain][objProductGroup]}"
                             var="objQP">
                    <apex:variable value="{!objQP.BigMachines__Product__c};{!objQP.Is_Overage__c}" var="uniqueKey"
                                   />
                    <apex:variable value="{!map_ExistingOriginalSubscrProds[uniqueKey].Product_Quantity__c}"
                                   var="orgQty" rendered="{!objQuote.Order_Type__c == $Label.Amend_Subscription || objQuote.Order_Type__c == $Label.Renew_Subscription}"
                                   />
                    <apex:variable value="{!map_ExistingOriginalSubscrProds[uniqueKey].Ext_Net_Price__c}"
                                   var="orgExtPrice" rendered="{!objQuote.Order_Type__c == $Label.Amend_Subscription || objQuote.Order_Type__c == $Label.Renew_Subscription}"
                                   />
                    <tr style="text-align:center;">
                        <td style="text-align:left;">
                            <apex:outputText value="{!objQP.Name + ' (' + objQP.APN_Number__c + ')'}"
                                             />
                        </td>
                        <td style="text-align:left;">
                            <apex:outputText value="{!objQP.BigMachines__Product__r.Unit_of_Measure__c}"
                                             />
                        </td>
                        <td style="{!IF(objQuote.Order_Type__c == $Label.Amend_Subscription && objProductGroupMain != 'Non Recurring Fees', '', 'display:none')}">
                            <apex:outputText value="{!IF(orgQty != null, orgQty, 0)}" />
                        </td>
                        <td>
                            <apex:outputText value="{!IF((objQuote.Order_Type__c == $Label.Amend_Subscription) && objProductGroupMain=='Non Recurring Fees', objQP.Quantity_Change__c,  objQP.Product_Quantity__c)}"
                                             />
                        </td>
                        <td style="{!IF(objQuote.Hide_List_Price__c, 'display:none', '')}">
                            <!--Mukunth updated for 6 digit change for PEC-->
                            <!--<apex:outputText value="{!strCurrencySymbol}{0, number, ###,##0.00}">
<apex:param value="{!objQP.List_Price__c}"/>
</apex:outputText>-->
                            <apex:outputText value="{!strCurrencySymbol}{0, number,###,###,##0.00}" rendered="{!mapProdToPrecision[objQP.BigMachines__Product__c].Tier_Pricing_Decimal_Precision__c == null || mapProdToPrecision[objQP.BigMachines__Product__c].Tier_Pricing_Decimal_Precision__c == '2'}">
                                <apex:param value="{!objQP.List_Price__c}" />
                            </apex:outputText>
                            <apex:outputText value="{!strCurrencySymbol}{0, number,###,###,##0.0000}" rendered="{!mapProdToPrecision[objQP.BigMachines__Product__c].Tier_Pricing_Decimal_Precision__c == '4'}">
                                <apex:param value="{!objQP.List_Price__c}" />
                            </apex:outputText>
                            <apex:outputText value="{!strCurrencySymbol}{0, number,###,###,##0.000000}" rendered="{!mapProdToPrecision[objQP.BigMachines__Product__c].Tier_Pricing_Decimal_Precision__c == '6'}">
                                <apex:param value="{!objQP.List_Price__c}" />
                            </apex:outputText>
                            <!--Mukunth code end-->
                        </td>
                        <td style="{!IF(objQuote.BigMachines__Opportunity__r.Indirect_Partner_PureCloud_Approved__c, '', 'display:none')}">
                            <!--Mukunth updated for 6 digit change for PEC-->
                            <!--<apex:outputText value="{!strCurrencySymbol}{0, number, ###,##0.00}">
<apex:param value="{!objQP.Partner_Price__c}"/>
</apex:outputText>-->
                            <apex:outputText value="{!strCurrencySymbol}{0, number,###,###,##0.00}" rendered="{!mapProdToPrecision[objQP.BigMachines__Product__c].Tier_Pricing_Decimal_Precision__c == null || mapProdToPrecision[objQP.BigMachines__Product__c].Tier_Pricing_Decimal_Precision__c == '2'}">
                                <apex:param value="{!objQP.Partner_Price__c}" />
                            </apex:outputText>
                            <apex:outputText value="{!strCurrencySymbol}{0, number,###,###,##0.0000}" rendered="{!mapProdToPrecision[objQP.BigMachines__Product__c].Tier_Pricing_Decimal_Precision__c == '4'}">
                                <apex:param value="{!objQP.Partner_Price__c}" />
                            </apex:outputText>
                            <apex:outputText value="{!strCurrencySymbol}{0, number,###,###,##0.000000}" rendered="{!mapProdToPrecision[objQP.BigMachines__Product__c].Tier_Pricing_Decimal_Precision__c == '6'}">
                                <apex:param value="{!objQP.Partner_Price__c}" />
                            </apex:outputText>
                            <!--Mukunth code end-->
                        </td>
                        <td style="{!IF(objQuote.Hide_Discount__c, 'display:none', '')}">
                            <apex:outputField value="{!objQP.Total_Discount__c}" />
                        </td>
                        <td>
                            <!--Mukunth updated for 6 digit change for PEC-->
                            <!--<apex:outputText value="{!strCurrencySymbol}{0, number, ###,##0.00}">
<apex:param value="{!objQP.Net_Unit_Price__c}"/>
</apex:outputText>-->
                            <apex:outputText value="{!strCurrencySymbol}{0, number,###,###,##0.00}" rendered="{!mapProdToPrecision[objQP.BigMachines__Product__c].Tier_Pricing_Decimal_Precision__c == null || mapProdToPrecision[objQP.BigMachines__Product__c].Tier_Pricing_Decimal_Precision__c == '2'}">
                                <apex:param value="{!objQP.Net_Unit_Price__c}" />
                            </apex:outputText>
                            <apex:outputText value="{!strCurrencySymbol}{0, number,###,###,##0.0000}" rendered="{!mapProdToPrecision[objQP.BigMachines__Product__c].Tier_Pricing_Decimal_Precision__c == '4'}">
                                <apex:param value="{!objQP.Net_Unit_Price__c}" />
                            </apex:outputText>
                            <apex:outputText value="{!strCurrencySymbol}{0, number,###,###,##0.000000}" rendered="{!mapProdToPrecision[objQP.BigMachines__Product__c].Tier_Pricing_Decimal_Precision__c == '6'}">
                                <apex:param value="{!objQP.Net_Unit_Price__c}" />
                            </apex:outputText>
                            <!--Mukunth code end-->
                        </td>
                        <td>
                            <apex:outputText value="{!strCurrencySymbol}{0, number, ###,##0.00}" rendered="{!((objProductGroupMain!='Subscription Fees' && objQuote.Billing_Category__c=='M2M') || objQuote.Billing_Category__c!='M2M')}">
                                <apex:param value="{!IF((objQuote.Order_Type__c == $Label.Amend_Subscription) && objProductGroupMain=='Non Recurring Fees', objQP.Quantity_Change__c * objQP.Net_Unit_Price__c, 
                                                   IF(objProductGroupMain=='Non Recurring Fees' || objProductGroupMain=='Overage Subscription Pricing', objQP.Ext_Net_Price__c, objQP.Extended_Price_Per_Period__c))}"
                                            />
                            </apex:outputText>
                            <apex:outputText value="Based on Usage" rendered="{!(objProductGroupMain=='Subscription Fees' && objQuote.Billing_Category__c=='M2M')}"
                                             />
                        </td>
                    </tr>
                </apex:repeat>
            </table>
            <br/><br/>
        </apex:repeat>
    </apex:repeat>
    
    <!--Removed Charges -- Starts -->
    <apex:outputPanel rendered="{!NOT(objQuote.Show_One_Time_Fees_Only__c) && objQuote.Order_Type__c == $Label.Amend_Subscription && objQuote.Amendment_For__c != null && list_RemovedProducts.size > 0}">
        <apex:outputText value="Removed Charges" styleClass="subHeader" />
        <br/><br/>
        <table cellspacing="1" cellpadding="5" border="1" style="border: 1px solid black;border-collapse:collapse;page-break-before:auto;page-break-inside:avoid;"
               width="100%">
            <tr class="tableHeader" style="font-size:9pt;font-family:'Calibri';">
                <td colspan="15">Information</td>
            </tr>
            <tr style="font-weight:bold;text-align:center;">
                <th width="20%">Product</th>
                <th width="8%">Quantity</th>
                <th>Unit</th>
                <th style="{!IF(objQuote.Hide_List_Price__c, 'display:none', '')}">List Price</th>
                <th style="{!IF(objQuote.Hide_Discount__c, 'display:none', '')}">Discount (%)</th>
                <th>Net Unit Price</th>
                <th>Extended Price</th>
            </tr>
            <apex:repeat value="{!list_RemovedProducts}" var="objSP">
                <tr style="text-align:center;{!IF(NOT(objSP.Hide_Record__c), '', 'display:none')}">
                    <td>
                        <apex:outputText value="{!objSP.Product__r.Name}" />
                    </td>
                    <td>
                        <apex:outputText value="{!objSP.Product_Quantity__c}" />
                    </td>
                    <td>
                        <apex:outputText value="{!objSP.Product__r.Unit_of_Measure__c}"
                                         />
                    </td>
                    <td style="{!IF(objQuote.Hide_List_Price__c, 'display:none', '')}">
                        <apex:outputText value="{!strCurrencySymbol}{0, number, ###,##0.0000}">
                            <apex:param value="{!objSP.List_Price__c}" />
                        </apex:outputText>
                    </td>
                    <td style="{!IF(objQuote.Hide_Discount__c, 'display:none', '')}">
                        <apex:outputField value="{!objSP.Total_Discount__c}" />
                    </td>
                    <td>
                        <apex:outputText value="{!strCurrencySymbol}{0, number, ###,##0.0000}">
                            <apex:param value="{!objSP.Net_Unit_Price__c}" />
                        </apex:outputText>
                    </td>
                    <td>
                        <apex:outputText value="{!strCurrencySymbol}{0, number, ###,##0.00}">
                            <apex:param value="{!objSP.Ext_Net_Price__c}" />
                        </apex:outputText>
                    </td>
                </tr>
            </apex:repeat>
        </table>
        <br/><br/>
    </apex:outputPanel>
    <!--Removed Charges -- Ends -->
    
    <!--Original Charges -- Starts -->
    <apex:outputPanel rendered="{!NOT(objQuote.Show_One_Time_Fees_Only__c) && objQuote.Order_Type__c == $Label.Amend_Subscription && objQuote.Amendment_For__c != null && !objQuote.Show_Updated_Quantity_Only__c}">
        <apex:outputText value="Original Charges" styleClass="subHeader" />
        <br/><br/>
        <table cellspacing="1" cellpadding="5" border="1" style="border: 1px solid black;border-collapse:collapse;page-break-before:auto;page-break-inside:avoid;"
               width="100%">
            <tr class="tableHeader" style="font-size:9pt;font-family:'Calibri';">
                <td colspan="15">Information</td>
            </tr>
            <tr style="font-weight:bold;text-align:center;">
                <th width="20%">Product</th>
                <th width="8%">Quantity</th>
                <th>Unit</th>
                <th style="{!IF(objQuote.Hide_List_Price__c, 'display:none', '')}">List Price</th>
                <th style="{!IF(objQuote.Hide_Discount__c, 'display:none', '')}">Discount (%)</th>
                <th>Net Unit Price</th>
                <th>Extended Price</th>
            </tr>
