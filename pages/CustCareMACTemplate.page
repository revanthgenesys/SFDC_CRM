<apex:page standardController="Case" extensions="MACTemplatecontroller" sidebar="false" >
 
 
 <apex:form id="frmid">     
   <apex:pageBlock id="pgblk" >   
       
     <apex:outputLabel value="Create a MAC (Move, Add, Change) Request" /> 
     
     <br/>
     <apex:outputText value="Welcome to the MAC Wizard. Please choose from one of the options below." style="font-style:italic; font-size:small;"/>
     <br/>
     <apex:outputText value="There may be some options which do not apply to your implementation of Interaction Center. 
       Please take this into consideration when making your choice. Thank You." style="font-style:italic; font-size:small;"/>
       
      <apex:pageMessages id="messages"/>  
           
     <apex:pageBlockSection columns="1" >
         <apex:inputField value="{!cs.MAC_Type_PureCnct__c}" label="MAC Type">
              <apex:actionSupport event="onchange" reRender="frmid"/>  
         </apex:inputField>
      </apex:pageBlockSection>
 
      <apex:inputField value="{!cs.Problem_Sub_Type__c}" id="subtp"/>
      <script>
            document.getElementById("{!$Component.subtp}").hidden=true;
      </script>
     
    <!-- #############################01.Add A User####################### -->
        <apex:pageBlockSection columns="1"  rendered="{!IF(cs.MAC_Type_PureCnct__c == 'Add A User',true,false)}" id="adduserb"   >
            <apex:pageBlockSectionItem > 
           <b>User First Name</b> &nbsp; 
              <apex:outputPanel >
                <div class="requiredInput">
                    <div class="requiredBlock"></div>
                    <apex:inputText value="{!FirstName}" label="User First Name" style="width: 235px;" rendered="true"/>
                </div>
            </apex:outputPanel>
            </apex:pageBlockSectionItem>
            <apex:pageBlockSectionItem > 
           <b>User Last Name</b> &nbsp; 
              <apex:outputPanel >
                <div class="requiredInput">
                    <div class="requiredBlock"></div>
            <apex:inputText label="User Last Name" value="{!LastName}" style="width: 235px;"  />     
            </div>
            </apex:outputPanel>
            </apex:pageBlockSectionItem>
            <apex:pageBlockSectionItem > 
             <b>User ID (must be unique)</b> &nbsp; 
              <apex:outputPanel >
                <div class="requiredInput">
                    <div class="requiredBlock"></div>    
            <apex:inputText label="User ID (must be unique)" value="{!UserID}" style="width: 235px;" />      
            </div>
            </apex:outputPanel>
            </apex:pageBlockSectionItem>
            <apex:pageBlockSectionItem > 
             <b>User Password</b> &nbsp; 
              <apex:outputPanel >
                <div class="requiredInput">
                    <div class="requiredBlock"></div>    
            <apex:inputText label="User Password" value="{!Password}" style="width: 235px; "  />      
            </div>
            </apex:outputPanel>
            </apex:pageBlockSectionItem>
            <apex:pageBlockSectionItem > 
             <b>User Extension</b> &nbsp; 
              <apex:outputPanel >
                <div class="requiredInput">
                    <div class="requiredBlock"></div>    
            <apex:inputText label="User Extension" value="{!Extension}"  style="width: 235px;"  />      
            </div>
            </apex:outputPanel>
            </apex:pageBlockSectionItem>        
            <apex:pageBlockSectionItem > 
             <b>User can activate self?</b> &nbsp; 
              <apex:outputPanel >
                <div class="requiredInput">
                    <div class="requiredBlock"></div>    
             <apex:selectList label="User can activate self?" value="{!SelfActivation}"     size="1">
                <apex:selectOption itemValue="" itemLabel=""/> 
                <apex:selectOption itemValue="Yes" itemLabel="Yes"/>   
                <apex:selectOption itemValue="No" itemLabel="No"/>   
            </apex:selectList>    
            </div>
            </apex:outputPanel>
            </apex:pageBlockSectionItem>    
            <apex:pageBlockSectionItem > 
             <b>Maximum Calls?</b> &nbsp; 
              <apex:outputPanel >
                <div class="requiredInput">
                    <div class="requiredBlock"></div>    
             <apex:selectList label="Maximum Calls" value="{!MaximumCalls}"  size="1"   >
                <apex:selectOption itemValue="" itemLabel=""/>  
                <apex:selectOption itemValue="0" itemLabel="0"/>  
                <apex:selectOption itemValue="1" itemLabel="1"/>   
                <apex:selectOption itemValue="2" itemLabel="2"/>   
            </apex:selectList>   
            </div>
            </apex:outputPanel>
            </apex:pageBlockSectionItem>    
            <apex:pageBlockSectionItem > 
