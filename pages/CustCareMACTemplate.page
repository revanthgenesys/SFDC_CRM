            </apex:pageBlockSectionItem>    
              
            <apex:inputTextarea label="Additional Instructions Regarding This MAC/User" value="{!AdditionalInstructions}" style="width: 360px; height: 100px" />  
       
        </apex:pageBlockSection>                

     
