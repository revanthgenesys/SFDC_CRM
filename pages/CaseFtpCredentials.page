<apex:page Controller="CaseFtpCredentials" showHeader="false">
     <apex:form >
       <apex:pageBlock >
           <apex:pageblockSection columns="2">
             <apex:outputLabel ><b>FTP User Name</b></apex:outputLabel>
                 <apex:outputText value="{!user}"/>
             <apex:outputLabel ><b>FTP Password</b></apex:outputLabel>
                 <apex:outputText value="{!pswd}"/>
           </apex:pageblockSection>
       </apex:pageBlock>
     </apex:form>
</apex:page>